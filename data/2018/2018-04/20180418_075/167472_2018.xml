               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Organisation des Endkundenservice</AC_CRITERION>
                  <AC_WEIGHTING>10</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Technisches Konzept des Netzbetriebes</AC_CRITERION>
                  <AC_WEIGHTING>20</AC_WEIGHTING>
               </AC_QUALITY>
