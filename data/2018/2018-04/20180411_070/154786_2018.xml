         <OBJECT_CONTRACT>
            <TITLE>
               <P>MR-kontrastmedel</P>
            </TITLE>
            <REFERENCE_NUMBER>UC-2017-321</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="33600000"/>
