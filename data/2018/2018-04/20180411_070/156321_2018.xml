                     <AC_CRITERION>Quality</AC_CRITERION>
                     <AC_WEIGHTING>50</AC_WEIGHTING>
                  </AC_QUALITY>
                  <AC_PRICE>
                     <AC_WEIGHTING>50</AC_WEIGHTING>
                  </AC_PRICE>
               </AC>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
               <INFO_ADD>
                  <P>See tender documents for further information.</P>
               </INFO_ADD>
            </OBJECT_DESCR>
