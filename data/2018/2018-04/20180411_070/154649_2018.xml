            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Neubeschaffung eines Patientenüberwachungssystems</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <n2016:NUTS CODE="DE712"/>
               <MAIN_SITE>
                  <P>Ginnheimer Straße 3</P>
                  <P>60487 Frankfurt am Main</P>
               </MAIN_SITE>
               <SHORT_DESCR>
                  <P>Erweiterung des Gebäudebestandes der Patientenüberwachung für folgende Abteilungen:</P>
                  <P>— Intensivstation,</P>
                  <P>— Aufwachbereich.</P>
                  <P>Die Neubeschaffung umfasst die folgenden Stückzahlen:</P>
                  <P>10 St.Patientenüberwachungsmonitore (Intensivstation)</P>
                  <P>10 St. Patientenüberwachungsmonitore (Aufwachbereich)</P>
                  <P>20 St. Transportmonitore</P>
                  <P>3 St. Module für die Herzzeitvolumen-Messung</P>
                  <P>1 St. Patientenüberwachungszentrale</P>
                  <P>3 St. Wandbefestigte Spiegelmonitore</P>
                  <P>1 St. Spiegelmonitor, Standbefestigung</P>
                  <P>4 St. Lautsprecher</P>
               </SHORT_DESCR>
               <AC_COST>
                  <AC_CRITERION>Preis</AC_CRITERION>
                  <AC_WEIGHTING>70 %</AC_WEIGHTING>
               </AC_COST>
               <AC_COST>
                  <AC_CRITERION>Bewertungskomitee</AC_CRITERION>
                  <AC_WEIGHTING>30 %</AC_WEIGHTING>
               </AC_COST>
               <DATE_START>2018-06-22</DATE_START>
               <DATE_END>2018-08-31</DATE_END>
               <NO_RENEWAL/>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
         </OBJECT_CONTRACT>
