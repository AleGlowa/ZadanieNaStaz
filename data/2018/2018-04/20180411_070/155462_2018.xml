               <AC_PRICE>
                  <AC_WEIGHTING>30 %</AC_WEIGHTING>
               </AC_PRICE>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
