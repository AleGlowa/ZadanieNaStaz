         <OBJECT_CONTRACT>
            <TITLE>
               <P>Maintenance et aménagement des aires de jeux de la ville de Palaiseau</P>
            </TITLE>
            <REFERENCE_NUMBER>2018-07</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="50870000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Le présent accord-cadre concerne des prestations de maintenance et d'aménagement des aires de jeux qui appartiennent à la ville de Palaiseau</P>
            </SHORT_DESCR>
         </OBJECT_CONTRACT>
         <COMPLEMENTARY_INFO>
            <DATE_DISPATCH_NOTICE>2018-04-09</DATE_DISPATCH_NOTICE>
