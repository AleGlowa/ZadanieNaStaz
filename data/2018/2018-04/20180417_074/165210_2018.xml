               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>70,00</AC_WEIGHTING>
               </AC_PRICE>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
