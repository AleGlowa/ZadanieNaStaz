            <TITLE>
               <P>IT služby - návrh analýzy informačného systému</P>
            </TITLE>
            <REFERENCE_NUMBER>001/VO/2018</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="72212461"/>
