         <OBJECT_CONTRACT>
            <TITLE>
               <P>Rammeaftale for etablering af flyverstik i Helsingør</P>
            </TITLE>
            <CPV_MAIN>
               <CPV_CODE CODE="09323000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="WORKS"/>
            <SHORT_DESCR>
               <P>Forsyning Helsingør ønsker at tilkoble nye private kunder og industri kunder til det eksisterende fjernvarmenet i Helsingør.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="45231100"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
