            <OBJECT_DESCR ITEM="1">
               <TITLE>
                  <P>Fourniture et pose de bâches d'information extérieures et intérieures</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="39522110"/>
