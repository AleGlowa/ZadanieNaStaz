            <OBJECT_DESCR ITEM="1">
               <TITLE>
                  <P>Elektroinstallationsarbeiten Starkstrom</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="45311200"/>
