            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="79713000"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="HU231"/>
               <n2016:NUTS CODE="HU110"/>
