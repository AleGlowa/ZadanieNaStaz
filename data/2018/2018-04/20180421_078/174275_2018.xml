         <OBJECT_CONTRACT>
            <TITLE>
               <P>Clarion Housing's Digital Experience Strategy, Design and Implementation</P>
            </TITLE>
            <REFERENCE_NUMBER>Clarion Ref: 2600-2018-KR</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="72413000"/>
