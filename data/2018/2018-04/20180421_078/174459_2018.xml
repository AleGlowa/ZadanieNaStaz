         <OBJECT_CONTRACT>
            <TITLE>
               <P>Oproep tot aanvraag tot deelneming - Groepsaankoop hospitalisatieverzekering</P>
            </TITLE>
            <REFERENCE_NUMBER>2018/02RE</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="66000000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Zie II.2.4.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
