               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="BE213"/>
               <SHORT_DESCR>
                  <P>Groepsaankoop hospitalisatieverzekering.</P>
               </SHORT_DESCR>
               <AC_PRICE/>
               <DURATION TYPE="MONTH">48</DURATION>
