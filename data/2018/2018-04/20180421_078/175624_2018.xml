               <VAL_OBJECT CURRENCY="NOK">20000000.00</VAL_OBJECT>
               <DATE_START>2018-09-01</DATE_START>
               <DATE_END>2020-08-31</DATE_END>
               <RENEWAL/>
               <RENEWAL_DESCR>
                  <P>The agreement has a duration of 2 years + 1 year + 1 year.</P>
               </RENEWAL_DESCR>
               <NO_ACCEPTED_VARIANTS/>
