               <SHORT_DESCR>
                  <P>Delaftale 1: Vogne med tilbehør (barnevogne, klapvogne o.l. med tilbehør, såsom insektnet, regnslag o.l.).</P>
               </SHORT_DESCR>
               <AC_PROCUREMENT_DOC/>
               <DURATION TYPE="MONTH">24</DURATION>
               <RENEWAL/>
