            <OBJECT_DESCR ITEM="1">
               <TITLE>
                  <P>Elaboration du plan local d'urbanisme intercommunal</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="71240000"/>
