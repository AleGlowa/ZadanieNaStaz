                  <AC_CRITERION>Pertinence des intentions relatives aux techniques spéciales</AC_CRITERION>
                  <AC_WEIGHTING>20</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Pertinence des intentions relatives à la faisabilité de la réalisation du projet (budget travaux, planning et sécurité)</AC_CRITERION>
                  <AC_WEIGHTING>20</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>0</AC_WEIGHTING>
               </AC_PRICE>
