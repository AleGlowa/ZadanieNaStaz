               <VAL_OBJECT CURRENCY="EUR">405297.79</VAL_OBJECT>
               <DURATION TYPE="DAY">80</DURATION>
               <NO_RENEWAL/>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
