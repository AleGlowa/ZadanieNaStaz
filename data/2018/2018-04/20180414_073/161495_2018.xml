            <TITLE>
               <P>Beschaffung eines Bandsicherungssystems</P>
            </TITLE>
            <CPV_MAIN>
               <CPV_CODE CODE="30210000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Beschaffung eines Bandsicherungssystems.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="51612000"/>
