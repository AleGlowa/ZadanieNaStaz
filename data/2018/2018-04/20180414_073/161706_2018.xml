         <OBJECT_CONTRACT>
            <TITLE>
               <P>Gastro technológia kuchyne</P>
            </TITLE>
            <REFERENCE_NUMBER>k CRD - 1863/2017/2018</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="39312000"/>
