               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Doświadczenie zawodowe kierownika budowy</AC_CRITERION>
                  <AC_WEIGHTING>5</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>60</AC_WEIGHTING>
               </AC_PRICE>
               <NO_OPTIONS/>
               <EU_PROGR_RELATED>Numer identyfikacyjny projektu: RPZP.09.01.00-32-0003/17-00</EU_PROGR_RELATED>
            </OBJECT_DESCR>
