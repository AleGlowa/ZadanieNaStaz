               <OPTIONS/>
               <OPTIONS_DESCR>
                  <P>Vereiste opties (Vereiste opties voor preventief onderhoudscontract).</P>
               </OPTIONS_DESCR>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
         </OBJECT_CONTRACT>
