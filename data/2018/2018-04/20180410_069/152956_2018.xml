            <OBJECT_DESCR ITEM="1">
               <TITLE>
                  <P>4 Stück EWA OS Hybrid 2018</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="34114000"/>
