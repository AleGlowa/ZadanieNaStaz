            <CPV_MAIN>
               <CPV_CODE CODE="79311200"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Przedmiotem zamówienia jest wykonanie kompleksowych badań ruchu we Wrocławiu i jego okolicach, który został podzielony na 7 etapów.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="PLN">2032510.00</VAL_TOTAL>
            <NO_LOT_DIVISION/>
