         <OBJECT_CONTRACT>
            <TITLE>
               <P>Fourniture, installation et exploitation d'un nouveau système de télécommunication UCC et système de vidéoconférence</P>
            </TITLE>
            <REFERENCE_NUMBER>PO17033</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="64200000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
