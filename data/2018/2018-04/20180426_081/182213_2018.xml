         <OBJECT_CONTRACT>
            <TITLE>
               <P>Levering, installatie en exploitatie van een nieuw Unified Communication- en videoconferentiesysteem</P>
            </TITLE>
            <REFERENCE_NUMBER>PO17033</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="64200000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
