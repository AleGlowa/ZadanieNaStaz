            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Le marché se compose de 2 lots, chaque lot donnant lieu à un marché, conformément aux dispositions de l'article 12 du décret nº 2016-360 du 25.3.2016.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">250522</VAL_TOTAL>
            <LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <TITLE>
                  <P>Maintenance des ascenseurs et monte-charges</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="42416100"/>
