            <PT_COMPETITIVE_NEGOTIATION/>
            <CONTRACT_COVERED_GPA/>
            <DATE_RECEIPT_TENDERS>2018-05-25</DATE_RECEIPT_TENDERS>
            <TIME_RECEIPT_TENDERS>12:00</TIME_RECEIPT_TENDERS>
            <DATE_DISPATCH_INVITATIONS>2018-05-29</DATE_DISPATCH_INVITATIONS>
            <LANGUAGES>
