               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="79212500"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="NL"/>
               <SHORT_DESCR>
                  <P>Zie markconsultatiedocument.</P>
               </SHORT_DESCR>
            </OBJECT_DESCR>
            <DATE_PUBLICATION_NOTICE>2018-09-01</DATE_PUBLICATION_NOTICE>
