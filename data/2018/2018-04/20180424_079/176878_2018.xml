            <SHORT_DESCR>
               <P>Appui à l'animation du réseau rural et péri-urbain d'Île-de-France.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="79952000"/>
               </CPV_ADDITIONAL>
