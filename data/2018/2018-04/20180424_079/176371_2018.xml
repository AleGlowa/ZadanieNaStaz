               </AC_PRICE>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
            <OBJECT_DESCR ITEM="3">
               <TITLE>
                  <P>Pérennisation des infrastructures Wi-Fi et des réseaux de commutateurs industriels</P>
               </TITLE>
               <LOT_NO>3</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="50312310"/>
               </CPV_ADDITIONAL>
