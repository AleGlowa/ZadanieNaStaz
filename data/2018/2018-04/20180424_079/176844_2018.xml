         <OBJECT_CONTRACT>
            <TITLE>
               <P>Begegnungs- und Dienstleistungsstätte im Zentrum Bröbberow</P>
            </TITLE>
            <REFERENCE_NUMBER>2018/S 045-098709</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="71240000"/>
