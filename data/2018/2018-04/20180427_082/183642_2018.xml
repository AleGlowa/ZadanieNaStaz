            <OBJECT_DESCR ITEM="1">
               <TITLE>
                  <P>Fourniture de poubelles publiques</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="34928400"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="BE332"/>
               <MAIN_SITE>
                  <P>Master Park de Boncelles.</P>
               </MAIN_SITE>
               <SHORT_DESCR>
                  <P>Ce lot vise la fourniture de 15 poubelles d'extérieur dotées d'un cendrier et d'un couvercle pour empêcher l'eau de pluie de rentrer dans la poubelle.</P>
               </SHORT_DESCR>
               <AC_PROCUREMENT_DOC/>
               <DATE_END>2018-08-31</DATE_END>
               <NO_RENEWAL/>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
            <OBJECT_DESCR ITEM="2">
               <TITLE>
                  <P>Fourniture de bancs publiques</P>
               </TITLE>
               <LOT_NO>2</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="34928400"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="BE332"/>
               <MAIN_SITE>
                  <P>Master Park de Boncelles.</P>
               </MAIN_SITE>
               <SHORT_DESCR>
                  <P>Ce lot vise la fourniture de 14 bancs d'extérieur.</P>
               </SHORT_DESCR>
               <AC_PROCUREMENT_DOC/>
               <DATE_END>2018-08-31</DATE_END>
