         <OBJECT_CONTRACT>
            <TITLE>
               <P>Master Park — Fourniture de divers équipements pour l'aménagement d'espaces verts</P>
            </TITLE>
            <REFERENCE_NUMBER>2018-3230</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="34928400"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Voir II.1.1.</P>
