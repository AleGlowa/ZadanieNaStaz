               <TITLE>
                  <P>Fourniture et pose de bancs de type «transat»</P>
               </TITLE>
               <LOT_NO>4</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="34928400"/>
               </CPV_ADDITIONAL>
