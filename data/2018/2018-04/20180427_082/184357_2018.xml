            <REFERENCE_NUMBER>Cultuurconnect-201801-Cultuurconnect-F02_0</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="72200000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
