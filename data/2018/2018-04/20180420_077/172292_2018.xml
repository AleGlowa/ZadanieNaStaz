         <OBJECT_CONTRACT>
            <TITLE>
               <P>Design and Installation for the Customer Experience (catering and social spaces)</P>
            </TITLE>
            <REFERENCE_NUMBER>CAT11029-TU</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="45212500"/>
