            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>1 darab Illumina MiSeq system új generációs szekvenáló készülék</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="HUF">51755890</VAL_TOTAL>
