         <OBJECT_CONTRACT>
            <TITLE>
               <P>Aankoop elektro-Coagulatoren voor elektrochirurgie</P>
            </TITLE>
            <REFERENCE_NUMBER>2017/233/AKAZSTM</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="33161000"/>
            </CPV_MAIN>
