         <OBJECT_CONTRACT>
            <TITLE>
               <P>Wykonanie usługi prania bielizny szpitalnej i odzieży ochronnej pracowników ZZOZ w Wadowicach</P>
            </TITLE>
            <REFERENCE_NUMBER>ZP/PRZET/12/2018</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="98310000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Przedmiotem zamówienia jest usługa prania bielizny szpitalnej i odzieży ochronnej pracowników.</P>
            </SHORT_DESCR>
            <VAL_ESTIMATED_TOTAL CURRENCY="PLN">1004184.00</VAL_ESTIMATED_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
