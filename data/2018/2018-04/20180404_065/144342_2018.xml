            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Achat d'un microscope à force atomique (AFM) avec modes électriques et module spécifique Scanning Microwave Impedance Microscopy (SMIM).</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="38510000"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="FRK24"/>
               <MAIN_SITE>
                  <P>Grenoble Minatec Laboratoire IMEP-LAHC.</P>
               </MAIN_SITE>
               <SHORT_DESCR>
                  <P>Achat d'un microscope à force atomique (AFM) avec modes électriques et module spécifique Scanning Microwave Impedance Microscopy (SMIM).</P>
               </SHORT_DESCR>
               <AC_QUALITY>
                  <AC_CRITERION>Valeur technique au vu du cadre réponse technique</AC_CRITERION>
                  <AC_WEIGHTING>60</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
