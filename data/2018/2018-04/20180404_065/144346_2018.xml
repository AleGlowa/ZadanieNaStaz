         <OBJECT_CONTRACT>
            <TITLE>
               <P>Fourniture et acheminement d'électricité avec services associés</P>
            </TITLE>
            <REFERENCE_NUMBER>Elec2019</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="09310000"/>
