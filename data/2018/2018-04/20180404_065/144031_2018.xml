                  <AC_WEIGHTING>60</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>40</AC_WEIGHTING>
               </AC_PRICE>
               <DURATION TYPE="MONTH">14</DURATION>
               <NO_RENEWAL/>
               <ACCEPTED_VARIANTS/>
               <OPTIONS/>
               <OPTIONS_DESCR>
