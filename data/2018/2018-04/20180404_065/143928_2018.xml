            <SHORT_DESCR>
               <P>Trockenbau, Doppelboden.</P>
            </SHORT_DESCR>
            <VAL_ESTIMATED_TOTAL CURRENCY="EUR">4731092.44</VAL_ESTIMATED_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="45421131"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
