            <SHORT_DESCR>
               <P>Wärme - und Kälteversorgung.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="42510000"/>
