               <AC_QUALITY>
                  <AC_CRITERION>Vraag ten aanzien van publicatie</AC_CRITERION>
                  <AC_WEIGHTING>10</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Wensen ten aanzien van casus 'Ten noorden van de Wadden'</AC_CRITERION>
                  <AC_WEIGHTING>45</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>30</AC_WEIGHTING>
               </AC_PRICE>
               <DURATION TYPE="MONTH">48</DURATION>
               <NO_RENEWAL/>
               <NO_ACCEPTED_VARIANTS/>
               <OPTIONS/>
               <OPTIONS_DESCR>
