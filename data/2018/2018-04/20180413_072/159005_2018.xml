               <DATE_START>2018-08-01</DATE_START>
               <DATE_END>2019-04-30</DATE_END>
               <NO_RENEWAL/>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
