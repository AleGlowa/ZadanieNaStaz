               <DURATION TYPE="MONTH">10</DURATION>
               <NO_RENEWAL/>
               <ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <EU_PROGR_RELATED>MyJustice Programme (DCI-ASIE/2014/037-333)</EU_PROGR_RELATED>
               <INFO_ADD>
