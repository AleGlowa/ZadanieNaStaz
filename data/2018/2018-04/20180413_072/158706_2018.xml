         <OBJECT_CONTRACT>
            <TITLE>
               <P>Sanierung ehemaliges Zusegymnasium zur Oberschule</P>
            </TITLE>
            <REFERENCE_NUMBER>I/60.21/18/26-VOB</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="45000000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="WORKS"/>
            <SHORT_DESCR>
               <P>Los 201 – Gerüstbau</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="45210000"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="45214200"/>
