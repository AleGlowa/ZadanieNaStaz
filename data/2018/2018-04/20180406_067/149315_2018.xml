                  <AC_WEIGHTING>40</AC_WEIGHTING>
               </AC_COST>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
         </OBJECT_CONTRACT>
