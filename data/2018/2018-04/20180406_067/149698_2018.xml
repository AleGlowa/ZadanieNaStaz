         <OBJECT_CONTRACT>
            <TITLE>
               <P>Dostawa trolejbusów oraz baterii trakcyjnych</P>
            </TITLE>
            <REFERENCE_NUMBER>5/S/2017</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="34622300"/>
