            <SHORT_DESCR>
               <P>Vergabeverfahren zur Lieferung eines Röntgendiffraktometers</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">1.00</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
