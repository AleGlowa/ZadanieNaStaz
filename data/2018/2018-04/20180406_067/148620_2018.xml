         <OBJECT_CONTRACT>
            <TITLE>
               <P>Zakup materiałów eksploatacyjnych i wyposażenia medycznego</P>
            </TITLE>
            <REFERENCE_NUMBER>WSPRiTS/ZP/48/17</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="33140000"/>
