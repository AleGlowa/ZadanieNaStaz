                  <AC_CRITERION>sistema operativo</AC_CRITERION>
                  <AC_WEIGHTING>50</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_COST>
                  <AC_CRITERION>prezzo</AC_CRITERION>
                  <AC_WEIGHTING>30</AC_WEIGHTING>
               </AC_COST>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
