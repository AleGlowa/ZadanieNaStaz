         <OBJECT_CONTRACT>
            <TITLE>
               <P>Oproep tot aanvraag tot deelneming - Trousses - Raamovereenkomst</P>
            </TITLE>
            <REFERENCE_NUMBER>2018/301</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="33140000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
