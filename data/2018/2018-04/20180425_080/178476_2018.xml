            <LOT_DIVISION>
               <LOT_COMBINING_CONTRACT_RIGHT>
                  <P>Separat tilldelning per anbudsområde.</P>
               </LOT_COMBINING_CONTRACT_RIGHT>
            </LOT_DIVISION>
