               <DATE_END>2018-09-30</DATE_END>
               <NO_RENEWAL/>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
         </OBJECT_CONTRACT>
