               </AC_COST>
               <AC_COST>
                  <AC_CRITERION>Umweltbetriebskosten</AC_CRITERION>
                  <AC_WEIGHTING>10</AC_WEIGHTING>
               </AC_COST>
               <DATE_START>2018-08-15</DATE_START>
               <DATE_END>2020-08-14</DATE_END>
               <NO_RENEWAL/>
               <ACCEPTED_VARIANTS/>
               <OPTIONS/>
               <OPTIONS_DESCR>
                  <P>Aufstockung der PKW-Anzahl von 22 auf bis zu 24</P>
               </OPTIONS_DESCR>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
         </OBJECT_CONTRACT>
