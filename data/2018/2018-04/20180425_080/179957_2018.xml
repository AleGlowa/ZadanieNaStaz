         <OBJECT_CONTRACT ITEM="1">
            <TITLE>
               <P>Dienstleistungen zur Sicherstellung der Baggergutbehandlung, Baggergutverwertung und Baggergutentsorgung</P>
            </TITLE>
            <CPV_MAIN>
               <CPV_CODE CODE="90611000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Ingenieurleistungen, Vermessungsleistungen, Planungsleistungen, Wartung- und Instandhaltungsleistungen, Unterhaltungsleistungen, Reinigungsleistungen und Winterdienst, Spülarbeiten zur Sicherstellung der Baggergutbehandlung, Baggergutverwertung und Baggergutentsorgung</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="71356400"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="71700000"/>
               </CPV_ADDITIONAL>
