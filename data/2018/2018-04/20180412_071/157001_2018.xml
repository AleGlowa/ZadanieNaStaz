               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="03340000"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="LV003"/>
               <MAIN_SITE>
                  <P>Rīga.</P>
               </MAIN_SITE>
               <SHORT_DESCR>
                  <P>Dzīvnieku identifikācijas līdzekļu piegāde.</P>
               </SHORT_DESCR>
               <AC_PRICE/>
