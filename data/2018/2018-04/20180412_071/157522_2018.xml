         <OBJECT_CONTRACT>
            <TITLE>
               <P>Microsoft Schulungsleistungen</P>
            </TITLE>
            <REFERENCE_NUMBER>ZIB 13.05 - 9935/17/VV: 1</REFERENCE_NUMBER>
            <CPV_MAIN>
