            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Norsk Tipping has entered into a contract with IGT Global Services Limited for the delivery of an Online Bingo System.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="NOK">25000000.00</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="48100000"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="72268000"/>
