               <NO_OPTIONS/>
               <EU_PROGR_RELATED>LSA Wissenschaft HS Bau Ausbau der anw oeff FuE Infrastruktur ZS 2016 12 82868</EU_PROGR_RELATED>
            </OBJECT_DESCR>
         </OBJECT_CONTRACT>
