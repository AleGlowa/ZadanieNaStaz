         <OBJECT_CONTRACT>
            <TITLE>
               <P>Oproep tot aanvraag tot deelneming - stents, ballonnen en hiermee gerelateerde verbruiksgoederen en apparaten - Raamovereenkomst</P>
            </TITLE>
            <REFERENCE_NUMBER>2018/294</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="33196000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
