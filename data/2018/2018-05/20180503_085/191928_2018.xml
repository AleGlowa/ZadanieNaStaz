         <OBJECT_CONTRACT>
            <TITLE>
               <P>Manned Guarding and Related Services</P>
            </TITLE>
            <REFERENCE_NUMBER>EFM2016NE</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="79710000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Lot 1 — Manned Guarding Services.</P>
               <P>Lot 2 — CCTV Monitoring Services.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="GBP">8000000</VAL_TOTAL>
