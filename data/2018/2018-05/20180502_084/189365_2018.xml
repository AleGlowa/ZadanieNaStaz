                  <AC_CRITERION>Dienstverlening en leveringsmodaliteiten.</AC_CRITERION>
                  <AC_WEIGHTING>25</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>55</AC_WEIGHTING>
               </AC_PRICE>
               <DURATION TYPE="MONTH">12</DURATION>
               <RENEWAL/>
               <RENEWAL_DESCR>
                  <P>Niet gespecifieerd.</P>
               </RENEWAL_DESCR>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
