         <OBJECT_CONTRACT>
            <TITLE>
               <P>Organisation des élections des représentants des locataires pour le groupe 3F</P>
            </TITLE>
            <REFERENCE_NUMBER>ELEC-LOC</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="79570000"/>
