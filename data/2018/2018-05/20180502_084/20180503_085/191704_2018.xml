         <OBJECT_CONTRACT>
            <TITLE>
               <P>Management and Operation of Former Plaza Cinema, Port Talbot</P>
            </TITLE>
            <REFERENCE_NUMBER>NPT - 1403</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="70220000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Neath Port Talbot County Borough Council is seeking a suitable organisation to lease and manage the soon to be re-furbished former Plaza Cinema, situated at Talbot Road, Port Talbot, SA13 1DH.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="70220000"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
