            <OBJECT_DESCR ITEM="1">
               <TITLE>
                  <P>Gestion intégrée du courrier entrant — sortant</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="64121000"/>
               </CPV_ADDITIONAL>
