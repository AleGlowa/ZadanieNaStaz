                  <P>Gestion des flux entrants et sortants du courrier et traitement des chèques.</P>
               </SHORT_DESCR>
               <AC_QUALITY>
                  <AC_CRITERION>Valeur technique de l'offre</AC_CRITERION>
                  <AC_WEIGHTING>50</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>50</AC_WEIGHTING>
               </AC_PRICE>
               <DURATION TYPE="MONTH">12</DURATION>
               <RENEWAL/>
