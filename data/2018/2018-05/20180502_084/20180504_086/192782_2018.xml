         <OBJECT_CONTRACT>
            <TITLE>
               <P>Sanierung ehemaliges Zusegymnasium zur Oberschule</P>
            </TITLE>
            <REFERENCE_NUMBER>I/60.21/18/05-VOB</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="45000000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="WORKS"/>
            <SHORT_DESCR>
               <P>Los 105 – Außenfenster, Außentüren – Metall</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">128209.00</VAL_TOTAL>
            <NO_LOT_DIVISION/>
