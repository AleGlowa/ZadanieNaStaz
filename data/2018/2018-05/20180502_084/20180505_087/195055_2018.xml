            <SHORT_DESCR>
               <P>Gerätewagen Sanität für den Katastrophenschutz Sachsen-Anhalt – Fahrgestell, Aufbau, Einbauten, Beladung, Ausstattung gemäß DIN EN 1846 1-3 und E DIN 14502-2</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <n2016:NUTS CODE="DEE06"/>
               <MAIN_SITE>
                  <P>IBK Heyrothsberge</P>
                  <P>Biederitzer Str. 5</P>
                  <P>39175 Biederitz</P>
               </MAIN_SITE>
               <SHORT_DESCR>
                  <P>Beschaffung von 3 Gerätewagen Sanität mit Ausbau und Beladung</P>
               </SHORT_DESCR>
               <AC_PROCUREMENT_DOC/>
               <DATE_START>2018-07-23</DATE_START>
               <DATE_END>2019-04-16</DATE_END>
               <NO_RENEWAL/>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
