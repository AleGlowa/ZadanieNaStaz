         <OBJECT_CONTRACT>
            <TITLE>
               <P>Hjartdal New Care Centre, turnkey contract</P>
            </TITLE>
            <REFERENCE_NUMBER>18/12</REFERENCE_NUMBER>
            <CPV_MAIN>
