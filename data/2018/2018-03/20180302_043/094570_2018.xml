               <COUNTRY VALUE="SE"/>
            </ADDRESS_REVIEW_BODY>
            <DATE_DISPATCH_NOTICE>2018-03-01</DATE_DISPATCH_NOTICE>
         </COMPLEMENTARY_INFO>
