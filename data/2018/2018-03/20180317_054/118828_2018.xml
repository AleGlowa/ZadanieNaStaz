         <OBJECT_CONTRACT>
            <TITLE>
               <P>Historic Leinster House Refurbishment Project</P>
            </TITLE>
            <REFERENCE_NUMBER>105393</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="45200000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="WORKS"/>
            <SHORT_DESCR>
               <P>Refurbishment of Historic Leinster House.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">11301248.15</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="45000000"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="45210000"/>
