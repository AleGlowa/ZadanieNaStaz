         <OBJECT_CONTRACT>
            <TITLE>
               <P>Concours restreint de maîtrise d'œuvre Reconstruction du collège Jacques Ellul sur la commune de Bor</P>
            </TITLE>
            <REFERENCE_NUMBER>16fs-Jdcol-0001-U</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="71230000"/>
