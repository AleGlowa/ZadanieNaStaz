         <OBJECT_CONTRACT>
            <TITLE>
               <P>Milton Keynes College Campus Consolidation and Transformation</P>
            </TITLE>
            <REFERENCE_NUMBER>CA4695 -</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="71541000"/>
