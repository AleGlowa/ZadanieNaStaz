            <SHORT_DESCR>
               <P>Tender for a senior client representative and property development advisor.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
