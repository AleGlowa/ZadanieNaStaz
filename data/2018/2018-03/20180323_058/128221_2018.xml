                  <AC_WEIGHTING>0</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>40</AC_WEIGHTING>
               </AC_PRICE>
               <VAL_OBJECT CURRENCY="EUR">240000.00</VAL_OBJECT>
               <DURATION TYPE="MONTH">24</DURATION>
