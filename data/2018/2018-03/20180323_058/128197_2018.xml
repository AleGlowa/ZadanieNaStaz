               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="79512000"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="FR101"/>
               <SHORT_DESCR>
                  <P>Le lot a pour objet la fourniture de produits de téléphonie, l'installation et la configuration de ces produits, la maintenance et les formations associés.</P>
               </SHORT_DESCR>
               <AC_QUALITY>
                  <AC_CRITERION>Valeur technique (en points)</AC_CRITERION>
                  <AC_WEIGHTING>60</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>40</AC_WEIGHTING>
               </AC_PRICE>
               <DURATION TYPE="MONTH">48</DURATION>
               <NO_RENEWAL/>
