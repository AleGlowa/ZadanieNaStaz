            <OBJECT_DESCR ITEM="1">
               <n2016:NUTS CODE="UKK1"/>
               <MAIN_SITE>
                  <P>Gloucestershire, Wiltshire and Bristol/Bath area.</P>
               </MAIN_SITE>
               <SHORT_DESCR>
                  <P>Contractor logistic support for the Air defence system integrator.</P>
               </SHORT_DESCR>
               <DIRECTIVE_2009_81_EC>
                  <AC_PRICE/>
               </DIRECTIVE_2009_81_EC>
               <OPTIONS/>
               <OPTIONS_DESCR>
                  <P>The Contract shall include 2 6-month options.</P>
               </OPTIONS_DESCR>
