                  <AC_CRITERION>Kwaliteit</AC_CRITERION>
                  <AC_WEIGHTING>45</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Dienst na verkoop</AC_CRITERION>
                  <AC_WEIGHTING>5</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>50</AC_WEIGHTING>
               </AC_PRICE>
               <DURATION TYPE="MONTH">84</DURATION>
               <NO_RENEWAL/>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
         </OBJECT_CONTRACT>
