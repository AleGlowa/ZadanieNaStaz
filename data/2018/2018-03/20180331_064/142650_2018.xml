               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
               <INFO_ADD>
                  <P>All details relating to this competition are available on www.etenders.gov.ie Ref ID: 131954</P>
               </INFO_ADD>
            </OBJECT_DESCR>
