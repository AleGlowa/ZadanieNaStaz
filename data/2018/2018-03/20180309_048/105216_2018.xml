            <TITLE>
               <P>Lieferung eines StLF 20/25</P>
            </TITLE>
            <REFERENCE_NUMBER>FW R 1/2017</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="34144213"/>
