                  <AC_CRITERION>Criterios valorables en cifras o porcentajes</AC_CRITERION>
                  <AC_WEIGHTING>75</AC_WEIGHTING>
               </AC_COST>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
