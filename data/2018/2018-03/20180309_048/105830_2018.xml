               <OPTIONS_DESCR>
                  <P>Verlengingsoptie: 2 keer met 1 jaar (12 maanden).</P>
               </OPTIONS_DESCR>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
