            <OBJECT_DESCR ITEM="1">
               <n2016:NUTS CODE="ES120"/>
               <SHORT_DESCR>
                  <P>Servicio de ayuda a domicilio con fomento de la calidad en el empleo de la Fundación Municipal de Servicios Sociales del Ayuntamiento de Gijón.</P>
               </SHORT_DESCR>
               <VAL_OBJECT CURRENCY="EUR">20748393.5</VAL_OBJECT>
               <DURATION TYPE="MONTH">30</DURATION>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
