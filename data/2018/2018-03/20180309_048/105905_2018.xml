            <TITLE>
               <P>BÜ und Planprüfung Ingenieurbauwerke</P>
            </TITLE>
            <REFERENCE_NUMBER>A120270116</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="71300000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Bundesautobahnen A10/A24 AS Neuruppin bis AD Pankow im Zuge des Verfügbarkeitsmodells A10 / A24; Bauüberwachung und Planprüfung Ingenieurbauwerke.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">2370488.00</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="71300000"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="71320000"/>
