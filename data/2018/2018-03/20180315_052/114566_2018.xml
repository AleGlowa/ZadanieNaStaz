                  <AC_WEIGHTING>60</AC_WEIGHTING>
               </AC_PRICE>
               <NO_OPTIONS/>
               <EU_PROGR_RELATED>RPPM.10.02.01-22-0023/16 z dn. 28/06/2016</EU_PROGR_RELATED>
            </OBJECT_DESCR>
