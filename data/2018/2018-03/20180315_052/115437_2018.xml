               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Angebot einer täglichen Salatbar zum Selbstbedienen, mit Angabe des Aufpreises.</AC_CRITERION>
                  <AC_WEIGHTING>10</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>45</AC_WEIGHTING>
               </AC_PRICE>
               <DATE_START>2018-08-06</DATE_START>
               <DATE_END>2019-06-28</DATE_END>
