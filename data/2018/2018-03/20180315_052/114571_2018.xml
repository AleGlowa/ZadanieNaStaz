            <TITLE>
               <P>Samverkan/Partnering Aktivitetshall</P>
            </TITLE>
            <REFERENCE_NUMBER>UH-2017-276</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="45210000"/>
