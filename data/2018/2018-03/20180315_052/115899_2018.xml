            <TITLE>
               <P>Prestations de traiteurs pour les ministères économiques et financiers</P>
            </TITLE>
            <REFERENCE_NUMBER>traiteurs</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="55520000"/>
