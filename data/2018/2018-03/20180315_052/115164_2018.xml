         <OBJECT_CONTRACT>
            <TITLE>
               <P>Zakup, dostawa mebli medycznych.</P>
            </TITLE>
            <REFERENCE_NUMBER>25/2018</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="33000000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Zakup, dostawa mebli medycznych-przedmiot zamówienia zostal podzielony na pakiety:</P>
               <P>1 1 szt. - kozetka elektryczna</P>
               <P>2 3 szt. - wózek siedzący</P>
               <P>3 11 szt. - stojaki do kroplówek</P>
               <P>4 1 szt. - kozetka metalowa</P>
               <P>5 1 szt. - stolik - wózek medyczny</P>
               <P>6 2 szt. - wózek leżący</P>
               <P>7 4 szt. - wózek - lóżko do transportu i wybudzania</P>
               <P>8 1szt. - fotel do pobierania krwi</P>
               <P>9 4 szt. - taboret z oparciem</P>
               <P>10 1 szt. - wózek oddziałowy</P>
            </SHORT_DESCR>
         </OBJECT_CONTRACT>
         <COMPLEMENTARY_INFO>
            <DATE_DISPATCH_NOTICE>2018-03-13</DATE_DISPATCH_NOTICE>
