         <OBJECT_CONTRACT>
            <TITLE>
               <P>Provision of Water Framework Directive (WFD) related services 2018-2020</P>
            </TITLE>
            <REFERENCE_NUMBER>WI _ WFD 2018-2020</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="90700000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Waterways Ireland is seeking a consultant to undertake the Water Framework Directive canals monitoring programme from 2018 to 2020, with associated scientific analysis and reporting.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">179685.00</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="71600000"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="71620000"/>
