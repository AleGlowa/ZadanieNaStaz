                  <AC_WEIGHTING>40</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Methodology</AC_CRITERION>
                  <AC_WEIGHTING>30</AC_WEIGHTING>
               </AC_QUALITY>
