                  <P>Please refer to the ITT Document.</P>
               </SHORT_DESCR>
               <AC_QUALITY>
                  <AC_CRITERION>Quality</AC_CRITERION>
                  <AC_WEIGHTING>40 %</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>60 %</AC_WEIGHTING>
               </AC_PRICE>
               <NO_OPTIONS/>
