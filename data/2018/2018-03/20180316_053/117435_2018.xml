         <OBJECT_CONTRACT>
            <TITLE>
               <P>Sortering Plastic verpakkingen, Blik en Drinkpakken 2019 e.v. (MMO)</P>
            </TITLE>
            <REFERENCE_NUMBER>2018/MW/PV/04</REFERENCE_NUMBER>
            <CPV_MAIN>
