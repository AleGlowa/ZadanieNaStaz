            <SHORT_DESCR>
               <P>Upphandling av verksamhetssystem för socialförvaltningen i Staffanstorp.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="48330000"/>
