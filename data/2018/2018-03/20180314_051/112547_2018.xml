         <OBJECT_CONTRACT>
            <TITLE>
               <P>Dostawa urządzeń wielofunkcyjnych, peryferyjnych oraz stacji roboczych</P>
            </TITLE>
            <REFERENCE_NUMBER>WZP.2370.4.5.2018</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="30200000"/>
