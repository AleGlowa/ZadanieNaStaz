         <OBJECT_CONTRACT>
            <TITLE>
               <P>The Provision of Parking Enforcement Services</P>
            </TITLE>
            <REFERENCE_NUMBER>CPT2030</REFERENCE_NUMBER>
            <CPV_MAIN>
