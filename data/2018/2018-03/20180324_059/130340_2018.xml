               <n2016:NUTS CODE="BE254"/>
               <MAIN_SITE>
                  <P>Recyclageparken Mirom Menen en Mirom Roeselare.</P>
               </MAIN_SITE>
               <SHORT_DESCR>
                  <P>Dit perceel omvat de ophaling van afgewerkte olie van grasmachines en auto's.</P>
               </SHORT_DESCR>
               <AC_PRICE/>
               <DURATION TYPE="MONTH">12</DURATION>
               <RENEWAL/>
               <RENEWAL_DESCR>
                  <P>Niet gespecificeerd.</P>
               </RENEWAL_DESCR>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
