                  <P>Zie II.2.1.</P>
               </SHORT_DESCR>
               <AC_QUALITY>
                  <AC_CRITERION>Plan van aanpak inclusief beschrijving materiaal</AC_CRITERION>
                  <AC_WEIGHTING>40</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>60</AC_WEIGHTING>
               </AC_PRICE>
               <DURATION TYPE="MONTH">36</DURATION>
               <NO_RENEWAL/>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
