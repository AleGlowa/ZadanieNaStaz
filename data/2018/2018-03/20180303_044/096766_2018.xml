         <OBJECT_CONTRACT>
            <TITLE>
               <P>Inspection, calibration, maintenance and repair of Workshop Tools and Equipment</P>
            </TITLE>
            <REFERENCE_NUMBER>MWEU5</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="43800000"/>
