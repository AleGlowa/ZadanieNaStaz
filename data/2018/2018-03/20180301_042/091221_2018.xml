                  <AC_CRITERION>Délai d'exécution</AC_CRITERION>
                  <AC_WEIGHTING>26</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Valeur technique</AC_CRITERION>
                  <AC_WEIGHTING>20</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_COST>
