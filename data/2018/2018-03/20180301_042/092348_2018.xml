         <OBJECT_CONTRACT>
            <TITLE>
               <P>Provision of Health, Safety and Environmental Protection Delivery and Training</P>
            </TITLE>
            <REFERENCE_NUMBER>CB/BFG/0147</REFERENCE_NUMBER>
            <CPV_MAIN>
