            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Räumung von Kampfmitteln in neun Losen in den Regierungsbezirken Arnsberg, Detmold und Münster für den Zeitraum von 36 Monaten (1.2.2018 bis 31.1.2021)</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">0.90</VAL_TOTAL>
            <LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <TITLE>
                  <P>O I Oberflächendetektion</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="90523100"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="90523200"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="90522000"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
