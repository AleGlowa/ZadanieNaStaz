            <OBJECT_DESCR ITEM="1">
               <TITLE>
                  <P>Kontorsmöbler och Inredning</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="39100000"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="39200000"/>
