                  <AC_WEIGHTING>40 %</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>2. Demonstrable skills and competencies</AC_CRITERION>
                  <AC_WEIGHTING>40 %</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
