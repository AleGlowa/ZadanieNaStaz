         <OBJECT_CONTRACT>
            <TITLE>
               <P>Raamovereenkomst levering en installatie van aankondigingsschermen en gerelateerde beheerssoftware</P>
            </TITLE>
            <REFERENCE_NUMBER>2017/753</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="38653400"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Zie II.2.4.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="51611110"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
