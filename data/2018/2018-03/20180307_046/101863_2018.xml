            <SHORT_DESCR>
               <P>Neuverlegung Westringschluss zwischen Markkleeberg und Großzschocher in Leipzig.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="71322200"/>
