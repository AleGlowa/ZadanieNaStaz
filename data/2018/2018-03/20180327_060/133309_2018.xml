               </DIRECTIVE_2014_24_EU>
               <NO_OPTIONS/>
               <EU_PROGR_RELATED>RPWP.04.04.01-30-0007/16-00</EU_PROGR_RELATED>
            </OBJECT_DESCR>
         </OBJECT_CONTRACT>
