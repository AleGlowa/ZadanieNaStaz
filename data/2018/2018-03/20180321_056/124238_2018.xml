                  <AC_WEIGHTING>20</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Kwaliteit van concept- en visievorming van de opdracht</AC_CRITERION>
                  <AC_WEIGHTING>25</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_COST>
                  <AC_CRITERION>Honorarium</AC_CRITERION>
                  <AC_WEIGHTING>40</AC_WEIGHTING>
