                  <AC_WEIGHTING>10</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>40</AC_WEIGHTING>
               </AC_PRICE>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
