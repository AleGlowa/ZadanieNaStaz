         <PROCEDURE>
            <PT_COMPETITIVE_NEGOTIATION/>
            <CONTRACT_COVERED_GPA/>
            <DATE_RECEIPT_TENDERS>2018-04-24</DATE_RECEIPT_TENDERS>
