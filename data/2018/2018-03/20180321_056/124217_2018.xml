            <SHORT_DESCR>
               <P>Vállalkozási szerződés IT demonstrációs és élményközpont szakmai programjának megvalósítására.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="HUF">373531470</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="80310000"/>
               </CPV_ADDITIONAL>
