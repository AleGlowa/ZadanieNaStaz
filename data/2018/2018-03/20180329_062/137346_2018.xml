            <SHORT_DESCR>
               <P>Supply and installation of softwear and rental of reporting softwear for a 5 year period.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="GBP">218500.00</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
