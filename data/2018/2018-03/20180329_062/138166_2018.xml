            <PT_OPEN/>
            <DATE_RECEIPT_TENDERS>2018-04-27</DATE_RECEIPT_TENDERS>
            <TIME_RECEIPT_TENDERS>16:00</TIME_RECEIPT_TENDERS>
            <LANGUAGES>
