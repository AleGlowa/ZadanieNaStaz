               <n2016:NUTS CODE="UKM50"/>
               <MAIN_SITE>
                  <P>Aberdeenshire.</P>
               </MAIN_SITE>
               <SHORT_DESCR>
                  <P>The Authority requires Contractors appointed to this Framework Agreement to provide temporary accommodation for individuals or households who are homeless.</P>
               </SHORT_DESCR>
               <AC_PROCUREMENT_DOC/>
               <DURATION TYPE="MONTH">36</DURATION>
