            <OBJECT_DESCR ITEM="4">
               <TITLE>
                  <P>Progetto alcologia per patologie alcol-correlate, diagnosi e trattamento</P>
               </TITLE>
               <LOT_NO>4</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="85140000"/>
