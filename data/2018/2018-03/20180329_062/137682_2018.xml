                  <AC_CRITERION>caratteristiche tecnico-qualitative</AC_CRITERION>
                  <AC_WEIGHTING>100</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>0</AC_WEIGHTING>
               </AC_PRICE>
               <NO_OPTIONS/>
