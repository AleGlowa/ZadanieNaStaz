                  <AC_WEIGHTING>12</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Valeur technique: DOE et remise d'ouvrage</AC_CRITERION>
                  <AC_WEIGHTING>12</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Valeur technique: organisation de l'équipe pour le contrôle « qualité »</AC_CRITERION>
                  <AC_WEIGHTING>12</AC_WEIGHTING>
               </AC_QUALITY>
