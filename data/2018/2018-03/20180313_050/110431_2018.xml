            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Upphandlingen avser ramavtal gällande arbetskläder för måltid- städ och vårdpersonal.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="SEK">6300000</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="18110000"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
