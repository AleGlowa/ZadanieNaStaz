            <SHORT_DESCR>
               <P>Trafode ost ja paigaldus vastavalt hankedokumentides sätestatule.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="31170000"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="EE"/>
               <SHORT_DESCR>
                  <P>Trafode ost ja paigaldus vastavalt hankedokumentides sätestatule.</P>
               </SHORT_DESCR>
               <AC_PRICE/>
               <DURATION TYPE="MONTH">10</DURATION>
               <NO_RENEWAL/>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
