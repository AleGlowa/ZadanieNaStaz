            <TYPE_CONTRACT CTYPE="WORKS"/>
            <OBJECT_DESCR>
               <TITLE>
                  <P>Travaux d'ouvrage d'art, terrassement, assainissement, chaussées</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
