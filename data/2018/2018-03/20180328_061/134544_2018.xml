         <OBJECT_CONTRACT>
            <TITLE>
               <P>Gara carburante avio 3 — ID 1897.</P>
            </TITLE>
            <REFERENCE_NUMBER>ID 1897</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="09131000"/>
