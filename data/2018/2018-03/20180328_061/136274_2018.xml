         <OBJECT_CONTRACT>
            <TITLE>
               <P>Framework agreement for white goods and small electrical</P>
            </TITLE>
            <REFERENCE_NUMBER>2017/5566</REFERENCE_NUMBER>
            <CPV_MAIN>
