                  <AC_CRITERION>Délai de livraison à compter de la date de notification du marché</AC_CRITERION>
                  <AC_WEIGHTING>5</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>55</AC_WEIGHTING>
               </AC_PRICE>
               <DURATION TYPE="MONTH">24</DURATION>
               <NO_RENEWAL/>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
