                  <AC_WEIGHTING>20</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Moodboard bloemenkiosk</AC_CRITERION>
                  <AC_WEIGHTING>10</AC_WEIGHTING>
               </AC_QUALITY>
