                  <AC_WEIGHTING>50</AC_WEIGHTING>
               </AC_PRICE>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
         </OBJECT_CONTRACT>
