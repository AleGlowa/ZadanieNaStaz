         <OBJECT_CONTRACT>
            <TITLE>
               <P>UK SBS PR17186 Gravity Meter</P>
            </TITLE>
            <REFERENCE_NUMBER>UKSBSPR17186</REFERENCE_NUMBER>
            <CPV_MAIN>
