               <TITLE>
                  <P>Enterprise Mobility Management oplossing</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="64227000"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="NL"/>
               <MAIN_SITE>
                  <P>Ommen-Hardenberg.</P>
               </MAIN_SITE>
               <SHORT_DESCR>
                  <P>Eén oplossing voor de Bestuursdienst Ommen-Hardenberg.</P>
               </SHORT_DESCR>
               <AC_QUALITY>
                  <AC_CRITERION>Kwaliteit</AC_CRITERION>
                  <AC_WEIGHTING>60</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>40</AC_WEIGHTING>
               </AC_PRICE>
               <OPTIONS/>
