g.</P></SHORT_DESCRIPTION><CPV><CPV_MAIN><CPV_CODE CODE="71631000"/></CPV_MAIN></CPV><CONTRACT_COVERED_GPA VALUE="NO"/></DESCRIPTION_CONTRACT_AWARD_UTILITIES><COSTS_RANGE_AND_CURRENCY_WITH_VAT_RATE CURRENCY="EUR"><RANGE_VALUE_COST><LOW_VALUE FMTVAL="1900000.00">1 900 000,00</LOW_VALUE><HIGH_VALUE FMTVAL="2100000.00">2 100 000,00</HIGH_VALUE></RANGE_VALUE_COST><EXCLUDING_VAT/></COSTS_RANGE_AND_CURRENCY_WITH_VAT_RATE></OBJECT_CONTRACT_AWARD_UTILITIES><PROCEDURES_CONTRACT_AWARD_UTILITIES><TYPE_PROCEDURE_AWARD><PT_RESTRICTED/></TYPE_PROCEDURE_AWARD><F06_AWARD_CRITERIA_CONTRACT_UTILITIES_INFORMATION><PRICE_AWARD_CRITERIA PRICE="MOST_ECONOMICALLY_TENDER"/><NO_ELECTRONIC_AUCTION/></F06_AWARD_CRITERIA_CONTRACT_UTILITIES_INFORMATION><ADMINISTRATIVE_INFO_CONTRACT_AWARD_UTILITIES><REFERENCE_NUMBER_ATTRIBUTED><P>2017/S 019-032884</P></REFERENCE_NUMBER_ATTRIBUTED><PREVIOUS_PUBLICATION_INFORMATION_NOTICE_F6><PREVIOUS_PUBLICATION_EXISTS_F6><CNT_NOTICE_INFORMATION><CNT_NOTICE_INFORMATION_S CHOICE="CONTRACT_NOTICE"/><NOTICE_NUMBER_OJ>2017/S 019-032884</NOTICE_NUMBER_OJ><DATE_OJ><DAY>24</DAY><MONTH>01</MONTH><YEAR>2017</YEAR></DATE_OJ></CNT_NOTICE_INFORMATION><NO_OTHER_PREVIOUS_PUBLICATION/></PREVIOUS_PUBLICATION_EXISTS_F6></PREVIOUS_PUBLICATION_INFORMATION_NOTICE_F6></ADMINISTRATIVE_INFO_CONTRACT_AWARD_UTILITIES></PROCEDURES_CONTRACT_AWARD_UTILITIES><AWARD_CONTRACT_CONTRACT_AWARD_UTILITIES><AWARD_AND_CONTRACT_VALUE><DATE_OF_CONTRACT_AWARD><DAY>11</DAY><MONTH>04</MONTH><YEAR>2017</YEAR></DATE_OF_CONTRACT_AWARD><OFFERS_RECEIVED_NUMBER>6</OFFERS_RECEIVED_NUMBER><OFFERS_RECEIVED_NUMBER_MEANING>6</OFFERS_RECEIVED_NUMBER_MEANING><CONTACT_DATA_WITHOUT_RESPONSIBLE_NAME_CHP><ORGANISATION><OFFICIALNAME>DNV GL</OFFICIALNAME></ORGANISATION><COUNTRY VALUE="DE"/></CONTACT_DATA_WITHOUT_RESPONSIBLE_NAME_CHP><NO_CONTRACT_LIKELY_SUB_CONTRACTED/></AWARD_AND_CONTRACT_VALUE><AWARD_AND_CONTRACT_VALUE><DATE_OF_CONTRACT_AWARD><DAY>11</DAY><MONTH>04</MONTH><YEAR>2017</YEAR></DATE_OF_CONTRACT_AWARD><CONTACT_