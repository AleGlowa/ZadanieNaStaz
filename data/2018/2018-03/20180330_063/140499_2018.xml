            <SHORT_DESCR>
               <P>Sammlung, Beförderung, Umladung und Verwertung von Papier, Pappe und Kartonagen (PPK) aus dem Landkreis Altenkirchen.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">0.01</VAL_TOTAL>
            <LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <TITLE>
                  <P>Sammlung, Beförderung und Umladung von PPK</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
