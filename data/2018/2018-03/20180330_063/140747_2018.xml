               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="FI"/>
               <SHORT_DESCR>
                  <P>Ks. kohta II.1.4).</P>
               </SHORT_DESCR>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
         </OBJECT_CONTRACT>
