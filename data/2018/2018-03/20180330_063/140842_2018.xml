            <TYPE_CONTRACT CTYPE="WORKS"/>
            <SHORT_DESCR>
               <P>Verbouwing/renovatie station Arnhem Velperpoort, inclusief aanpassing van het stationsplein en het spoor ter plaatse van het station.</P>
            </SHORT_DESCR>
         </OBJECT_CONTRACT>
         <COMPLEMENTARY_INFO>
            <DATE_DISPATCH_NOTICE>2018-03-29</DATE_DISPATCH_NOTICE>
            <NOTICE_NUMBER_OJ>2018/S 052-115938</NOTICE_NUMBER_OJ>
