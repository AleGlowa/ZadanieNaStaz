               </OPTIONS_DESCR>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
            <OBJECT_DESCR ITEM="4">
               <TITLE>
                  <P>Lot 4: Production de prothèses conjointes</P>
               </TITLE>
               <LOT_NO>4</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="85130000"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
