                  <AC_WEIGHTING>45</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>20</AC_WEIGHTING>
               </AC_PRICE>
               <VAL_OBJECT CURRENCY="EUR">1515000.00</VAL_OBJECT>
               <DURATION TYPE="MONTH">24</DURATION>
               <NO_RENEWAL/>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <EU_PROGR_RELATED>POR Puglia 2014-2020</EU_PROGR_RELATED>
