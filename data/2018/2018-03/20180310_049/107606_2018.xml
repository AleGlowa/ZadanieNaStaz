         <OBJECT_CONTRACT>
            <TITLE>
               <P>Ultrazvukové přístroje pro Krajskou zdravotní, a.s.</P>
            </TITLE>
            <REFERENCE_NUMBER>1125/2018</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="33112000"/>
