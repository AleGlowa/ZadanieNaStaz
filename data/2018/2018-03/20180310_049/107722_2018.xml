                  <AC_WEIGHTING>je 2,50 %</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_COST>
                  <AC_CRITERION>Preis</AC_CRITERION>
                  <AC_WEIGHTING>80,00 %</AC_WEIGHTING>
               </AC_COST>
               <AC_COST>
