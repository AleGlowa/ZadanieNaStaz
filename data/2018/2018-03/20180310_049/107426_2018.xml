               <TITLE>
                  <P>Petits objets publicitaires</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="39294100"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="FR"/>
               <SHORT_DESCR>
                  <P>— Stylos à bille - badges métallique rond - boite de mini-crayon s de couleurs - ballons de baudruche.</P>
               </SHORT_DESCR>
               <AC_PROCUREMENT_DOC/>
               <DURATION TYPE="MONTH">48</DURATION>
               <NO_RENEWAL/>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
