               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <EU_PROGR_RELATED>RPMP.02.01.01-12-0020/16</EU_PROGR_RELATED>
            </OBJECT_DESCR>
         </OBJECT_CONTRACT>
