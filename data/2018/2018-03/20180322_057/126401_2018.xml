                  <AC_WEIGHTING>25</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Moderation beteiligungsorientierter Projekte sowie sozialraumorientierter Netzwerkarbeit</AC_CRITERION>
                  <AC_WEIGHTING>20</AC_WEIGHTING>
               </AC_QUALITY>
