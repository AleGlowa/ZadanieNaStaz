         <OBJECT_CONTRACT>
            <TITLE>
               <P>Centre aquatique intercommunal à Flixecourt</P>
            </TITLE>
            <REFERENCE_NUMBER>2018-AOR-Piscine</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="45212212"/>
