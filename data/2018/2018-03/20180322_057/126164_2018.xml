         <OBJECT_CONTRACT>
            <TITLE>
               <P>Nadgradnja sistema IJPP in vzdrževanje aplikacij</P>
            </TITLE>
            <REFERENCE_NUMBER>430-48/2017</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="72540000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Nadgradnja sistema IJPP in vzdrževanje aplikacij.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="48610000"/>
               </CPV_ADDITIONAL>
