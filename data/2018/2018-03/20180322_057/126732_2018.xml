         <OBJECT_CONTRACT>
            <TITLE>
               <P>Provision of Minor Hand Surgery</P>
            </TITLE>
            <REFERENCE_NUMBER>LOC0007</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="85100000"/>
