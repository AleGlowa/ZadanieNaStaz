               <n2016:NUTS CODE="NL"/>
               <MAIN_SITE>
                  <P>Netherlands.</P>
               </MAIN_SITE>
               <SHORT_DESCR>
                  <P>2.</P>
               </SHORT_DESCR>
            </OBJECT_DESCR>
            <DATE_PUBLICATION_NOTICE>2018-04-10</DATE_PUBLICATION_NOTICE>
