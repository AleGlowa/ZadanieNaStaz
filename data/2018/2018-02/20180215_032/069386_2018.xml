         <OBJECT_CONTRACT>
            <TITLE>
               <P>Storköksutrustning, Ks 1256/2017</P>
            </TITLE>
            <REFERENCE_NUMBER>Ks 1256/2017</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="39314000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Upphandling för Örebro län av diskmaskiner, kylskåp, spisar, diverse vagnar ill storkök, grönsaksskärare m m.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="39221000"/>
               </CPV_ADDITIONAL>
