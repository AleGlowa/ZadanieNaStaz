         <OBJECT_CONTRACT>
            <TITLE>
               <P>Mise en œuvre de services régionaux de gestion de l'imagerie médicale et de rapprochement d'identité</P>
            </TITLE>
            <REFERENCE_NUMBER>TSA_KRYPTON_DC_01</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="48000000"/>
