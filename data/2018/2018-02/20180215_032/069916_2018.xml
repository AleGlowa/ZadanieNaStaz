               <DATE_START>2018-04-23</DATE_START>
               <DATE_END>2019-04-22</DATE_END>
               <RENEWAL/>
               <RENEWAL_DESCR>
                  <P>Option to extend for a further 2 x 1 year periods.</P>
               </RENEWAL_DESCR>
               <ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
