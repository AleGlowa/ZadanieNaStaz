            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Servizio triennale, prorogabile per un max di 6 mesi, di copertura assicurativa RCT-RCO.</P>
            </SHORT_DESCR>
            <VAL_ESTIMATED_TOTAL CURRENCY="EUR">4550000.00</VAL_ESTIMATED_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
