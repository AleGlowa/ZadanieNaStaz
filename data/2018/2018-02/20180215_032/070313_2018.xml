                  <AC_CRITERION>Bewertungsmatrix</AC_CRITERION>
                  <AC_WEIGHTING>100 %</AC_WEIGHTING>
               </AC_COST>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
