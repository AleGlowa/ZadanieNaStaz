            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Beratungsleistungen für das Projekt Strategische Neuausrichtung des Einkaufs untergliedert in zwei Teilprojekte.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">750375.00</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="79400000"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="79418000"/>
