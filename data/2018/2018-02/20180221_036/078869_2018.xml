                  <AC_CRITERION>Grundzüge CMS-Strategie</AC_CRITERION>
                  <AC_WEIGHTING>7,5</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Qualität des Supports</AC_CRITERION>
                  <AC_WEIGHTING>10</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>50</AC_WEIGHTING>
               </AC_PRICE>
               <OPTIONS/>
               <OPTIONS_DESCR>
                  <P>Optionale Supportstunden,</P>
                  <P>Optionale Umsetzungen und Weiterentwicklungen des Loses.</P>
               </OPTIONS_DESCR>
               <NO_EU_PROGR_RELATED/>
