         <COMPLEMENTARY_INFO>
            <ADDRESS_REVIEW_BODY>
               <OFFICIALNAME>Vergabekammer des Bundes</OFFICIALNAME>
               <ADDRESS>Villemomblerstr. 76</ADDRESS>
               <TOWN>Bonn</TOWN>
               <POSTAL_CODE>53123</POSTAL_CODE>
