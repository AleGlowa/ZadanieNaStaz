               <COUNTRY VALUE="SE"/>
            </ADDRESS_REVIEW_BODY>
            <DATE_DISPATCH_NOTICE>2018-02-19</DATE_DISPATCH_NOTICE>
         </COMPLEMENTARY_INFO>
