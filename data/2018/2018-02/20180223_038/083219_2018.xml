                  <AC_WEIGHTING>5</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Plan de calidad</AC_CRITERION>
                  <AC_WEIGHTING>5</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Medidas de calidad y seguridad</AC_CRITERION>
                  <AC_WEIGHTING>20</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_COST>
