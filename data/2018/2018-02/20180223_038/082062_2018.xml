                  <AC_WEIGHTING>40</AC_WEIGHTING>
               </AC_COST>
               <VAL_OBJECT CURRENCY="EUR">12000000</VAL_OBJECT>
               <DURATION TYPE="MONTH">12</DURATION>
               <RENEWAL/>
               <RENEWAL_DESCR>
                  <P>Nombre de reconductions éventuelles: 3 calendrier prévisionnel des marchés ultérieurs: 12 mois (à compter de la date de notification).</P>
               </RENEWAL_DESCR>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
