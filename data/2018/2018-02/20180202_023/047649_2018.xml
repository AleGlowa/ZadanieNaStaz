            </OBJECT_DESCR>
            <OBJECT_DESCR ITEM="2">
               <TITLE>
                  <P>O - Rampeentreprisen</P>
               </TITLE>
               <LOT_NO>2</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="45221110"/>
