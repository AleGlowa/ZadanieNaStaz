               <DATE_END>2022-07-31</DATE_END>
               <NO_RENEWAL/>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
