            <SHORT_DESCR>
               <P>Beschaffung eines Inversen Mikroskopstatives mit Multispektraldetektor und verbesserter Auflösung durch Airyscan-Detector.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">1.00</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <n2016:NUTS CODE="DE30"/>
               <MAIN_SITE>
                  <P>10117 Berlin</P>
               </MAIN_SITE>
               <SHORT_DESCR>
                  <P>Beschaffung eines Inversen Mikroskopstatives mit Multispektraldetektor und verbesserter Auflösung durch Airyscan-Detector.</P>
               </SHORT_DESCR>
               <AC_QUALITY>
                  <AC_CRITERION>Erfüllung der wissenschaftlichen Anforderungen</AC_CRITERION>
                  <AC_WEIGHTING>100</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>0</AC_WEIGHTING>
               </AC_PRICE>
               <NO_OPTIONS/>
