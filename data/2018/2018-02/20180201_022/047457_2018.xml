                  <AC_CRITERION>Competence</AC_CRITERION>
                  <AC_WEIGHTING>70</AC_WEIGHTING>
               </AC_COST>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
