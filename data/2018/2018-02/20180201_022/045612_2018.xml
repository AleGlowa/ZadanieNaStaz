                  <AC_CRITERION>Durée de garantie</AC_CRITERION>
                  <AC_WEIGHTING>25</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>50</AC_WEIGHTING>
               </AC_PRICE>
