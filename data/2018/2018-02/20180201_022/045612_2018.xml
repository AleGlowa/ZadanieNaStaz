            <CPV_MAIN>
               <CPV_CODE CODE="34922000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Fourniture d'une machine d'application de produits de marquage routier.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="34922000"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="FR102"/>
               <SHORT_DESCR>
                  <P>Fourniture d'une machine d'application de produits de marquage routier le délai maximum d'exécution des prestations est fixé à 6 mois.</P>
               </SHORT_DESCR>
               <AC_QUALITY>
                  <AC_CRITERION>Valeur technique</AC_CRITERION>
                  <AC_WEIGHTING>25</AC_WEIGHTING>
               </AC_QUALITY>
