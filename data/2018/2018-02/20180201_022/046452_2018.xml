            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>IT-Unterstützung des SAP CCoE in der Bundesagentur für Arbeit.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">0.01</VAL_TOTAL>
