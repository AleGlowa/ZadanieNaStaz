                  <AC_CRITERION>MEAT — Quality</AC_CRITERION>
                  <AC_WEIGHTING>55</AC_WEIGHTING>
               </AC_COST>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
