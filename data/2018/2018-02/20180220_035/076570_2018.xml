            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="90500000"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="FRF24"/>
               <MAIN_SITE>
                  <P>Sarcicourt et Montlandon en Haute-Marne pour le pompage des lixiviats.</P>
               </MAIN_SITE>
