         <OBJECT_CONTRACT>
            <TITLE>
               <P>Tierce maintenance applicative «XTI» et prestations associées</P>
            </TITLE>
            <REFERENCE_NUMBER>TMA-XTI</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="72260000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Le ministère de la Justice souhaite confier à une entreprise la maintenance de son groupe applicatif «XTI» qui regroupe les applications et logiciels informatiques destinés à la gestion de l'activité des juridictions civiles.</P>
            </SHORT_DESCR>
            <VAL_ESTIMATED_TOTAL CURRENCY="EUR">2800000</VAL_ESTIMATED_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="72267100"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="FR"/>
               <SHORT_DESCR>
                  <P>Tierce maintenance du groupe applicatif «XTI» (support, maintenance standard, évolutive, adaptative) et prestations associées.</P>
               </SHORT_DESCR>
               <AC_PROCUREMENT_DOC/>
               <VAL_OBJECT CURRENCY="EUR">2800000</VAL_OBJECT>
               <DURATION TYPE="MONTH">72</DURATION>
               <NO_RENEWAL/>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
         </OBJECT_CONTRACT>
