         <OBJECT_CONTRACT>
            <TITLE>
               <P>Het verdiepen van kaaimuren in het Hansadok K246-K240</P>
            </TITLE>
            <REFERENCE_NUMBER>B10496</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="45240000"/>
