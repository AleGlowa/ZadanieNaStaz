            <TITLE>
               <P>Procurement of bone density measurers for Kristiansund and Ålesund Hospitals</P>
            </TITLE>
            <REFERENCE_NUMBER>2018/71</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="33100000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Bone density measurers for Kristiansund Hospital and Ålesund Hospital.</P>
            </SHORT_DESCR>
