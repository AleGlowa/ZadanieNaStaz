                  <AC_WEIGHTING>0</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>0</AC_WEIGHTING>
               </AC_PRICE>
               <OPTIONS/>
