         <OBJECT_CONTRACT ITEM="1">
            <TITLE>
               <P>Prior Information Notice for the provision of a National eProcurement Platform (NEP) managed and hosted by a third party on their premises as a Software as a Service (SaaS).</P>
            </TITLE>
            <REFERENCE_NUMBER>19756</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="48490000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Prior Information Notice for the provision of a National eProcurement Platform (NEP) managed and hosted by a third party on their premises as a Software as a Service (SaaS).</P>
            </SHORT_DESCR>
            <VAL_ESTIMATED_TOTAL CURRENCY="EUR">1.00</VAL_ESTIMATED_TOTAL>
