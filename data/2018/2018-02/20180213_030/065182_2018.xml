         <OBJECT_CONTRACT>
            <TITLE>
               <P>C-Trap en AFS</P>
            </TITLE>
            <CPV_MAIN>
               <CPV_CODE CODE="38433100"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Correlative Tweezers–Fluorescence Microscopy machine and an Acoustic Force Spectroscopy machine.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">347851.0</VAL_TOTAL>
            <NO_LOT_DIVISION/>
