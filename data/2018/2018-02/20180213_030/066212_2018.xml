         <OBJECT_CONTRACT>
            <TITLE>
               <P>GYSEV Zrt. részére közúti gépjárművek tartós bérlete és teljes körű flottarendszerű üzemeltetése</P>
            </TITLE>
            <CPV_MAIN>
               <CPV_CODE CODE="34100000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Keretszerződés a GYSEV Zrt. részére közúti gépjárművek tartós bérletére és teljes körű flottarendszerű üzemeltetésére.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="50111000"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="50111100"/>
               </CPV_ADDITIONAL>
