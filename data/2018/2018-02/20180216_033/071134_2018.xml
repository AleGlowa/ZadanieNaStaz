            <SHORT_DESCR>
               <P>Gebäudeautomation Schule Lindemannstraße.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="42131140"/>
