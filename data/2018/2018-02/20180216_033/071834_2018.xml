               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="72210000"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="79132100"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="UKN0"/>
               <SHORT_DESCR>
                  <P>Market engagement exploring the options available for the electronic transfer of prescriptions. Further information is available in the Request for Information.</P>
               </SHORT_DESCR>
            </OBJECT_DESCR>
            <DATE_PUBLICATION_NOTICE>2018-11-01</DATE_PUBLICATION_NOTICE>
