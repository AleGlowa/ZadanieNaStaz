         <OBJECT_CONTRACT ITEM="1">
            <TITLE>
               <P>Delivery Partner for Construction Works</P>
            </TITLE>
            <REFERENCE_NUMBER>1718-54-FDR-MG</REFERENCE_NUMBER>
            <CPV_MAIN>
