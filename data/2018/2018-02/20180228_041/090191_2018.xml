               <TITLE>
                  <P>Impression et livraison</P>
               </TITLE>
               <LOT_NO>3</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="79820000"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="FRD12"/>
               <MAIN_SITE>
                  <P>Cherbourg-en-Cotentin.</P>
