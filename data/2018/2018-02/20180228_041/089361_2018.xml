         <OBJECT_CONTRACT>
            <TITLE>
               <P>Mietvertrag für Multifunktionsgeräte für Verwaltung und Schulen der Stadt Görlitz</P>
            </TITLE>
            <REFERENCE_NUMBER>045.0131.04/2018</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="30121430"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Lieferung von Multifunktionsgeräten im Mietvertrag</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="30121430"/>
