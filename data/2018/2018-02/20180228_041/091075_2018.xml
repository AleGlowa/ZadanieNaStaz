            <TITLE>
               <P>The furnishing of Sandvika and Rykkinn Libraries</P>
            </TITLE>
            <REFERENCE_NUMBER>17/27451</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="39155000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>The furnishing of Sandvika and Rykkinn Libraries.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="NOK">7000000.00</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
