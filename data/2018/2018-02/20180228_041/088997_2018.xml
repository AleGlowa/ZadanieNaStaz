                  <P>VE 441 – Elektroinstallationsarbeiten</P>
               </SHORT_DESCR>
               <AC_PRICE/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
