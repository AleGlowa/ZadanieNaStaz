                  <CPV_CODE CODE="73000000"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="SE"/>
               <SHORT_DESCR>
                  <P>Se upphandlingsdokumentationen.</P>
               </SHORT_DESCR>
