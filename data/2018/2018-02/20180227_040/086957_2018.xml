         <OBJECT_CONTRACT>
            <TITLE>
               <P>Lieferung eines Gerätewagen Gefahrgut GW-G mit Kofferaufbau und Beladung</P>
            </TITLE>
            <REFERENCE_NUMBER>I/4/008</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="34144210"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Lieferung eines Gerätewagens für Gefahrgut:</P>
               <P>Los 1: Fahrgestell,</P>
               <P>Los 2: Kofferaufbau und Beladung.</P>
            </SHORT_DESCR>
            <VAL_ESTIMATED_TOTAL CURRENCY="EUR">420000.00</VAL_ESTIMATED_TOTAL>
            <LOT_DIVISION>
