         <OBJECT_CONTRACT>
            <TITLE>
               <P>Konsulttjänster avseende granskning av vårdgivares följsamhet till avtal och lag</P>
            </TITLE>
            <REFERENCE_NUMBER>HS 2017-00382</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="79100000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Konsulttjänster vid granskning av vårdgivares följsamhet till avtal och lag.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="79000000"/>
