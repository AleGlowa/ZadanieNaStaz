         <OBJECT_CONTRACT>
            <TITLE>
               <P>Personalised support and care in prisons and in approved premises in the County of Staffordshire</P>
            </TITLE>
            <REFERENCE_NUMBER>SCHG00167</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="85300000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>The Council wishes to commission Personalised Support and Care in Prisons and Approved Premises in the County of Staffordshire.</P>
            </SHORT_DESCR>
            <VAL_RANGE_TOTAL CURRENCY="GBP">
               <LOW>675000</LOW>
               <HIGH>1050000</HIGH>
            </VAL_RANGE_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="85310000"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="85312000"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="85320000"/>
               </CPV_ADDITIONAL>
