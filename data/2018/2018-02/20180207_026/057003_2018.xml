            <TITLE>
               <P>Leistungsbild Tragwerksplanung gemäß HOAI</P>
            </TITLE>
            <REFERENCE_NUMBER>17D0259</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="71327000"/>
