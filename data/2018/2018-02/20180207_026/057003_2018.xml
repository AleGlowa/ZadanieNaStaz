            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Tragwerksplanung für Gebäude, Leistungsbild HOAI 2013 für Neubau.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">629330.88</VAL_TOTAL>
