                  <AC_WEIGHTING>7</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Contact Centre – Call Routing</AC_CRITERION>
                  <AC_WEIGHTING>3</AC_WEIGHTING>
               </AC_QUALITY>
