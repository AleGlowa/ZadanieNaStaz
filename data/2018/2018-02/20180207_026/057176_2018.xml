               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>S5</AC_CRITERION>
                  <AC_WEIGHTING>100</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>Superformule</AC_WEIGHTING>
