            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>The competition concerns consultancy services for the communications and transport area for Aust og Vest Agder fylkeskommune, and for Adger kollektivtraffic AS distributed over 3 contract areas.</P>
            </SHORT_DESCR>
            <VAL_ESTIMATED_TOTAL CURRENCY="NOK">8000000.00</VAL_ESTIMATED_TOTAL>
            <LOT_DIVISION>
               <LOT_ALL/>
            </LOT_DIVISION>
            <OBJECT_DESCR ITEM="1">
               <TITLE>
                  <P>PART 1: DEVELOPMENT OF THE PUBLIC TRANSPORT FACILITIES.</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="71311200"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="71311210"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
