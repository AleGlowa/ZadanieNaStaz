               </RENEWAL_DESCR>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
            <OBJECT_DESCR ITEM="2">
               <TITLE>
                  <P>PART 2: DEVELOPMENT OF THE PUBLIC TRANSPORT SYSTEM.</P>
               </TITLE>
               <LOT_NO>2</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="71311200"/>
               </CPV_ADDITIONAL>
