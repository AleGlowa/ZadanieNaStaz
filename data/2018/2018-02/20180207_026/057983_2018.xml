               </RENEWAL_DESCR>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
            <OBJECT_DESCR ITEM="3">
               <TITLE>
                  <P>PART 3: ANALAYSES AND REVIEW OF THE TRANSPORT SYSTEM.</P>
               </TITLE>
               <LOT_NO>3</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="71311200"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
