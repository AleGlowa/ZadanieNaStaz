            <OBJECT_DESCR ITEM="3">
               <TITLE>
                  <P>Apparatuur en elektronische kasten voor acuut-technische afdelingen van AZ</P>
               </TITLE>
               <LOT_NO>3</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="42965110"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
