            <OBJECT_DESCR ITEM="2">
               <TITLE>
                  <P>Apparatuur en elektronische kasten voor op standaard verpleegafdelingen van AZ en PZ</P>
               </TITLE>
               <LOT_NO>2</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="42965110"/>
