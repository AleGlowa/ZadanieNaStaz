            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="34946110"/>
                  <CPV_SUPPLEMENTARY_CODE CODE="MA08"/>
               </CPV_ADDITIONAL>
