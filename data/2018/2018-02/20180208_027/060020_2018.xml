            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>1003701/1003701 PNN BUT</P>
               <P>Contract K 942 Display cases for a new National museum.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="NOK">80496882.00</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="39154000"/>
