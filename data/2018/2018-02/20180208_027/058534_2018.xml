            <OBJECT_DESCR ITEM="2">
               <TITLE>
                  <P>Betonblokk gyártó technológiai sor beszerzése hozzá kapcsolódó gyártó technológiával</P>
               </TITLE>
               <LOT_NO>2</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="42900000"/>
               </CPV_ADDITIONAL>
