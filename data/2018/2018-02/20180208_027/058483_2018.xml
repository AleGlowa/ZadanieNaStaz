         <OBJECT_CONTRACT>
            <TITLE>
               <P>Rahmenvereinbarung zur Lieferung, Montage und Instandhaltung medientechnischer Ausstattung</P>
            </TITLE>
            <REFERENCE_NUMBER>EU 01-2018</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="32320000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Rahmenvereinbarung über die Lieferung, Montage und Instandhaltung von medientechnischer Ausstattung.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="32321300"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="51313000"/>
