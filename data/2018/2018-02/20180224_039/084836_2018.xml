            <SHORT_DESCR>
               <P>Collectie foto's uit de Edwards Collectie.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="USD">550000.0</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <n2016:NUTS CODE="NL"/>
               <MAIN_SITE>
                  <P>Leiden.</P>
               </MAIN_SITE>
               <SHORT_DESCR>
                  <P>Totaal circa 3600 historische foto's uit Azië, zowel losse foto's als foto's in albums.</P>
               </SHORT_DESCR>
               <AC_PRICE/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
