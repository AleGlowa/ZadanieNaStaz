         <OBJECT_CONTRACT>
            <TITLE>
               <P>"További végponti eszközök - LORA kommunikációs adatgyűjtés" vállalkozási szerződés alapján.</P>
            </TITLE>
            <CPV_MAIN>
               <CPV_CODE CODE="32500000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>"További végponti eszközök - LORA kommunikációs adatgyűjtés" vállalkozási szerződés alapján.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">80371</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="65500000"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="38421100"/>
