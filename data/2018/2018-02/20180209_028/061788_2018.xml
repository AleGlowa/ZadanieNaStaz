                  </AC_PRICE>
               </AC>
               <OPTIONS/>
               <OPTIONS_DESCR>
                  <P>Mulighed for forlængelse af aftalen med 2x12 måneder.</P>
               </OPTIONS_DESCR>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
