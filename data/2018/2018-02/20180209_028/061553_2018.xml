         <OBJECT_CONTRACT>
            <TITLE>
               <P>Pakettiautojen ja kevytkuorma-autojen huoltoleasingpalvelut</P>
            </TITLE>
            <CPV_MAIN>
               <CPV_CODE CODE="34130000"/>
