                  <AC_CRITERION>Kwaliteit</AC_CRITERION>
                  <AC_WEIGHTING>50</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>50</AC_WEIGHTING>
               </AC_PRICE>
               <DATE_START>2018-06-01</DATE_START>
