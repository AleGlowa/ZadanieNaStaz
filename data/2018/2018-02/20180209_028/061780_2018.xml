         <OBJECT_CONTRACT>
            <TITLE>
               <P>Oceľové rúry DN1000</P>
            </TITLE>
            <REFERENCE_NUMBER>17-0296-RKSZ</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="44161100"/>
