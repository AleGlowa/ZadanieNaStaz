         <OBJECT_CONTRACT>
            <TITLE>
               <P>Usługa monitoringu systemów teletechnicznych w pomieszczeniu Control Room na terenie multimodalnego dworca kolejowego Łódź Fabryczna</P>
            </TITLE>
            <REFERENCE_NUMBER>KIZ/2017/WNP-WNP-007399</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="31620000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Usługa monitoringu systemów teletechnicznych w pomieszczeniu Control Room na terenie multimodalnego dworca kolejowego Łódź Fabryczna.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="32235000"/>
               </CPV_ADDITIONAL>
