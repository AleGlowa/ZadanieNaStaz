            <OBJECT_DESCR ITEM="3">
               <TITLE>
                  <P>Action de formation «Guide file et serre file»</P>
               </TITLE>
               <LOT_NO>3</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="79998000"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="FRE1"/>
