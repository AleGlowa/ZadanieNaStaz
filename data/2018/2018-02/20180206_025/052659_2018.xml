            <CPV_MAIN>
               <CPV_CODE CODE="45421000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="WORKS"/>
            <SHORT_DESCR>
               <P>Menuiserie extérieures et stores pour les travaux et la maintenance des bâtiments du Grand Port Maritime de Marseille.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">2500000</VAL_TOTAL>
            <LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <TITLE>
                  <P>Bassins Est et le Sauze: entretien et travaux de menuiseries extérieures dans les bâtiments du gpmm</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="45421000"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="FR"/>
               <SHORT_DESCR>
                  <P>Les travaux comprennent:</P>
                  <P>— la fourniture et pose en remplacement ou en création des menuiseries extérieures en aluminium, PVC, et bois,</P>
                  <P>— la fourniture et pose en remplacement ou en création de la vitrerie,</P>
                  <P>— la fourniture et la pose en remplacement ou en création de films de protection et de sécurité,</P>
                  <P>— la fourniture et la pose en remplacement ou en création de volets roulants et stores,</P>
                  <P>— la révision et réparation des menuiseries extérieures aluminium, PVC et bois, </P>
                  <P>— la révision, la réparation des stores et volets roulants,</P>
                  <P>— la fourniture et la pose en remplacement ou en création de garde-corps,</P>
                  <P>— la fourniture et pose en remplacement ou en création de quincaillerie et accessoires.</P>
               </SHORT_DESCR>
               <AC_QUALITY>
                  <AC_CRITERION>Valeur technique</AC_CRITERION>
                  <AC_WEIGHTING>40</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>60</AC_WEIGHTING>
               </AC_PRICE>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
