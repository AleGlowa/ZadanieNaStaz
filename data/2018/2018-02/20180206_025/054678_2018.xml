         <OBJECT_CONTRACT>
            <TITLE>
               <P>Architect Led Team for Gallery refurbishment from RIBA Stage 2 to 7</P>
            </TITLE>
            <CPV_MAIN>
               <CPV_CODE CODE="45453000"/>
               <CPV_SUPPLEMENTARY_CODE CODE="IA25"/>
