                  <AC_CRITERION>Beste prijs kwaliteit verhouding</AC_CRITERION>
                  <AC_WEIGHTING>1</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>1</AC_WEIGHTING>
               </AC_PRICE>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
