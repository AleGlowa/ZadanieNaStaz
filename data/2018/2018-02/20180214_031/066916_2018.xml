               <n2016:NUTS CODE="DE711"/>
               <MAIN_SITE>
                  <P>Grafenstraße 9,</P>
                  <P>64283 Darmstadt</P>
               </MAIN_SITE>
               <SHORT_DESCR>
                  <P>Heizung</P>
