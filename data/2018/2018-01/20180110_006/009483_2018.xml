         <OBJECT_CONTRACT>
            <TITLE>
               <P>Medizinische Versorgungsleistungen in der Landeseinrichtung für Asylbegehrende und Ausreisepflichtige (LEfAA) in Ingelheim</P>
            </TITLE>
            <REFERENCE_NUMBER>2017 / ZBL / Z.17-0063</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="85000000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Medizinische Versorgungsleistungen in der Landeseinrichtung für Asylbegehrende und Ausreisepflichtige (LEfAA) in Ingelheim</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">1854888.81</VAL_TOTAL>
