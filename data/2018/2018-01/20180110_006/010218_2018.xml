                     <AC_CRITERION>Marge</AC_CRITERION>
                     <AC_WEIGHTING>geen</AC_WEIGHTING>
                  </AC_COST>
               </AC>
               <OPTIONS/>
               <OPTIONS_DESCR>
                  <P>Er zijn twee verlengingopties van 1 jaar (of korter indien NS dit wenst).</P>
               </OPTIONS_DESCR>
               <NO_EU_PROGR_RELATED/>
               <INFO_ADD>
