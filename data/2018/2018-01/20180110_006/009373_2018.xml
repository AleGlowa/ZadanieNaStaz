                  <AC_CRITERION>Innovation</AC_CRITERION>
                  <AC_WEIGHTING>5</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Compliance</AC_CRITERION>
                  <AC_WEIGHTING>8</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Service Quality and Service Resiliance</AC_CRITERION>
                  <AC_WEIGHTING>3</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Implimentation and Training</AC_CRITERION>
                  <AC_WEIGHTING>2</AC_WEIGHTING>
