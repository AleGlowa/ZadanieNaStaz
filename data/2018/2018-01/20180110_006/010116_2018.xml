         <OBJECT_CONTRACT>
            <TITLE>
               <P>Rahmenvereinbarung über die Lieferung und Montage von Büromöbeln/-mobiliar einschließlich Abholung der Altmöbel</P>
            </TITLE>
            <REFERENCE_NUMBER>2017-00566</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="39130000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Lieferung und Montage von Büromöbeln/-mobiliar einschließlich der Abholung von Altmöbeln.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="39000000"/>
               </CPV_ADDITIONAL>
