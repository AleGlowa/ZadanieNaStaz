         <OBJECT_CONTRACT>
            <TITLE>
               <P>Izvedba projekta »Trajnostna mobilnost v vrtcih in osnovnih šolah«</P>
            </TITLE>
            <REFERENCE_NUMBER>430-47/2017</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="80000000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Predmet javnega naročila je izvedba projekta »Trajnostna mobilnost v vrtcih in osnovnih šolah«.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="63712710"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="71620000"/>
               </CPV_ADDITIONAL>
