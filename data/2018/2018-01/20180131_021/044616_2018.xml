            <TITLE>
               <P>Wykonywanie działań promujących oraz wspomagających rozwój społeczno-gospodarczy miasta Bydgoszczy</P>
            </TITLE>
            <REFERENCE_NUMBER>WZP.271.47.2017.E</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="79342200"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Wykonywanie działań promujących oraz wspomagających rozwój społeczno-gospodarczy miasta Bydgoszczy.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="PLN">1923902.44</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <n2016:NUTS CODE="PL613"/>
               <SHORT_DESCR>
                  <P>Wykonywanie działań promujących oraz wspomagających rozwój społeczno-gospodarczy miasta Bydgoszczy.</P>
               </SHORT_DESCR>
               <AC_PRICE/>
