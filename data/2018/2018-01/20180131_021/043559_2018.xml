            <TITLE>
               <P>Coventry Very Light Rail (VLR) Vehicle Project</P>
            </TITLE>
            <REFERENCE_NUMBER>CJ-01-18-LEP-CVLR2-DH</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="34000000"/>
