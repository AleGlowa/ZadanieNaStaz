            </LOT_DIVISION>
            <OBJECT_DESCR ITEM="1">
               <TITLE>
                  <P>Reklam och kommunikation</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="39294100"/>
               </CPV_ADDITIONAL>
