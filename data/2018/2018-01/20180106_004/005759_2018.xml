         <OBJECT_CONTRACT>
            <TITLE>
               <P>Wasserwerk Arberg, Los 12.1: Elektrotechnik Brunnen</P>
            </TITLE>
            <CPV_MAIN>
               <CPV_CODE CODE="45311000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="WORKS"/>
            <SHORT_DESCR>
               <P>Der Auftrag enthält folgende wesentlichen Leistungen:</P>
               <P>7 Stück Niederspannungsschaltanlage</P>
               <P>7 Stück Erdkabelanschlusskästen mit Blitz-/Überspannungsschutz</P>
               <P>200 m Gitter-/kabelrinnen</P>
               <P>7 SPS-Steuerungen (Hardware)</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR" PUBLICATION="YES">164583.50</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
