         <OBJECT_CONTRACT>
            <TITLE>
               <P>Hantverkstjänster - Taktäckning</P>
            </TITLE>
            <REFERENCE_NUMBER>115/16</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="45260000"/>
