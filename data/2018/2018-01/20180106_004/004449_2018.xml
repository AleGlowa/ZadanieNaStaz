         <OBJECT_CONTRACT>
            <TITLE>
               <P>WBO Dantestr. 6 in München, Techn. Gebäudeausrüstung HLS</P>
            </TITLE>
            <REFERENCE_NUMBER>SV-APE-170725-005</REFERENCE_NUMBER>
            <CPV_MAIN>
