            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="39200000"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="37500000"/>
