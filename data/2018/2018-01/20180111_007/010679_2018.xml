         <OBJECT_CONTRACT>
            <TITLE>
               <P>AI 2017-0342 Natuursteen herinrichting Nes</P>
            </TITLE>
            <REFERENCE_NUMBER>AI 2017-0342</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="45000000"/>
