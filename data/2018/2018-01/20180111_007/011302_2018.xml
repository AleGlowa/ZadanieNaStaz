               <n2016:NUTS CODE="FR103"/>
               <SHORT_DESCR>
                  <P>Fourniture de fictions pour la jeunesse comprenant des romans, des albums et des contes, en langues française et étrangère.</P>
               </SHORT_DESCR>
               <AC_QUALITY>
                  <AC_CRITERION>Valeur technique de l'offre</AC_CRITERION>
                  <AC_WEIGHTING>90</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
