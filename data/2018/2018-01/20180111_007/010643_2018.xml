         <OBJECT_CONTRACT>
            <TITLE>
               <P>Wirtschaftsgebäude | Erneuerung der Außenanlagen</P>
            </TITLE>
            <REFERENCE_NUMBER>18.003</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="45112700"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="WORKS"/>
            <SHORT_DESCR>
               <P>Im Rahmen der Sanierung der Außenanlagen des Wirtschaftsgebäudes des Universitätsklinikums Köln sind die Freiflächen des Wirtschaftshofes neu zu ordnen.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="45000000"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
