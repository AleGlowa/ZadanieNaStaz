            <OBJECT_DESCR ITEM="2">
               <TITLE>
                  <P>Dostawa, rozmieszczenie i wdrożenie urządzeń infrastruktury transmisji i przetwarzania danych</P>
               </TITLE>
               <LOT_NO>2</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="48800000"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="PL418"/>
