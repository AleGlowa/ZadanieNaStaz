            <TITLE>
               <P>Interne renovatie van 21 appartementen</P>
            </TITLE>
            <REFERENCE_NUMBER>2014/0340/02</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="45211000"/>
