         <COMPLEMENTARY_INFO>
            <ADDRESS_REVIEW_BODY>
               <OFFICIALNAME>Tribunal administratif de Melun</OFFICIALNAME>
               <ADDRESS>43 rue du général de Gaulle, case postale nº 8630</ADDRESS>
               <TOWN>Melun</TOWN>
               <POSTAL_CODE>77008</POSTAL_CODE>
               <COUNTRY VALUE="FR"/>
