         <OBJECT_CONTRACT>
            <TITLE>
               <P>Supply of Mechanised Tree Harvesters and Forwarders and Parts</P>
            </TITLE>
            <REFERENCE_NUMBER>FW0039</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="16600000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>The Forestry Commission awarded a framework agreement for the supply of mechanised tree harvesters and forwarders including associated parts for service and repair with delivery across Scotland and England.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="GBP">11000000</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
