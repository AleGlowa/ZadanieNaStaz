            <TITLE>
               <P>Optimisation de l'organisation automatisée d'un service de sérologie</P>
            </TITLE>
            <REFERENCE_NUMBER>DC 2018-1</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="48921000"/>
               <CPV_SUPPLEMENTARY_CODE CODE="LA41"/>
               <CPV_SUPPLEMENTARY_CODE CODE="IA01"/>
