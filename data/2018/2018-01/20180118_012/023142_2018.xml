            </LOT_DIVISION>
            <OBJECT_DESCR ITEM="1">
               <TITLE>
                  <P>Déchets non dangereux / collecte et recyclage de déchets papiers</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="90513000"/>
