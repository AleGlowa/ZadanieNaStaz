            <SHORT_DESCR>
               <P>Część I: Sukcesywne dostawy mięsa wieprzowego oraz wołowego.</P>
               <P>Część II: Sukcesywne dostawy wyrobów z mięsa wieprzowego oraz wołowego.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="PLN">2943061.00</VAL_TOTAL>
            <LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <TITLE>
                  <P>Sukcesywne dostawy mięsa wieprzowego oraz wołowego</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="15111100"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="15114000"/>
               </CPV_ADDITIONAL>
