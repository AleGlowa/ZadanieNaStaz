            <PT_RESTRICTED/>
            <CONTRACT_COVERED_GPA/>
            <NOTICE_NUMBER_OJ>2017/S 027-046965</NOTICE_NUMBER_OJ>
         </PROCEDURE>
         <AWARD_CONTRACT ITEM="1">
