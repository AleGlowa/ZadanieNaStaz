            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Rahmenvertrag über die Erbringung von Erstellungs- (Werk-) und Dienstleistungen für Softwareentwicklung NATURAL / ADABAS.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">0.01</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="72261000"/>
