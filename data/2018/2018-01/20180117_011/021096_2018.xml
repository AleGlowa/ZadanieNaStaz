         <OBJECT_CONTRACT>
            <TITLE>
               <P>Uspenskin katedraalin ikonostaasin konservointi- ja restaurointitöiden hankinta</P>
            </TITLE>
            <CPV_MAIN>
               <CPV_CODE CODE="92521210"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Hankinnan kohteena on Uspenskin katedraalin ikonostaasin konservointi- ja restaurointityöt.</P>
               <P>Hankinnan kohdetta on kuvattu tarkemmin hankinta-asiakirjoissa.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">700000</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="92522100"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="45212361"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="92311000"/>
