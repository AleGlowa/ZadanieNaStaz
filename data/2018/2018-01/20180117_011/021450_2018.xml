               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Retour géographique</AC_CRITERION>
                  <AC_WEIGHTING>15 %</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Administratif et contractuel</AC_CRITERION>
                  <AC_WEIGHTING>10 %</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>40 %</AC_WEIGHTING>
               </AC_PRICE>
