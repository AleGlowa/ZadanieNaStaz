         <COMPLEMENTARY_INFO>
            <DATE_DISPATCH_NOTICE>2018-01-12</DATE_DISPATCH_NOTICE>
            <NOTICE_NUMBER_OJ>2017/S 247-517649</NOTICE_NUMBER_OJ>
