         <OBJECT_CONTRACT>
            <TITLE>
               <P>Erneuerung Studiotechnik mit baulichen Anpassungen</P>
            </TITLE>
            <REFERENCE_NUMBER>2049721</REFERENCE_NUMBER>
            <CPV_MAIN>
