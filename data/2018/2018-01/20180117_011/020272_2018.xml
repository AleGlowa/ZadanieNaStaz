            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Erneuerung der Studiotechnik mit baulichen Anpassungen.</P>
            </SHORT_DESCR>
            <VAL_ESTIMATED_TOTAL CURRENCY="EUR">1100000.00</VAL_ESTIMATED_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="32300000"/>
