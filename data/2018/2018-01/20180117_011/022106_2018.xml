         <OBJECT_CONTRACT>
            <TITLE>
               <P>A Contract for Keys, Fittings and Locksmith Works and Material</P>
            </TITLE>
            <REFERENCE_NUMBER>17/05882</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="44500000"/>
