            <TITLE>
               <P>Neubau einer 4-zügigen Grundschule – Gewerk Schreinerarbeiten</P>
            </TITLE>
            <CPV_MAIN>
               <CPV_CODE CODE="45214210"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="WORKS"/>
            <SHORT_DESCR>
               <P>Neubau einer 4-zügigen Grundschule – Gewerk Schreinerarbeiten</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="45421141"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="45421130"/>
