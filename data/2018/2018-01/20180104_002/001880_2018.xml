            <TITLE>
               <P>Zakup legitymacji kolejowych uprawniających do korzystania ze zniżek 50 % na przejazdy PKP</P>
            </TITLE>
            <REFERENCE_NUMBER>ZP/6710/D/17</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="60210000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
            <SHORT_DESCR>
               <P>Zakup legitymacji kolejowych uprawniających do korzystania ze zniżek 50 % na przejazdy PKP.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="PLN">1176037.00</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <n2016:NUTS CODE="PL415"/>
               <SHORT_DESCR>
                  <P>Zakup legitymacji kolejowych uprawniających do korzystania ze zniżek 50 na przejazdy PKP.</P>
               </SHORT_DESCR>
               <AC_PRICE/>
