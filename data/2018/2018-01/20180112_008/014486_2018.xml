         <OBJECT_CONTRACT>
            <TITLE>
               <P>Raamovereenkomst voor de werken noodzakelijk voor de vernieuwing of uitbreiding van het stadswaterdistributienet en het brandbeveiligingsnet en de pompstations van het Brussels metronet</P>
            </TITLE>
            <REFERENCE_NUMBER>DITP-CSC-BB BM 1416-F06_0</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="45232150"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="WORKS"/>
            <SHORT_DESCR>
               <P>Zien punt I.I.4).</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR" PUBLICATION="YES">6805285.04</VAL_TOTAL>
