            <TITLE>
               <P>Levering van mobilofoniediensten ten behoeve van het Brussels Hoofdstedelijk Gewest en zijn mandaatgevers</P>
            </TITLE>
            <REFERENCE_NUMBER>CIRB-CSC2017.040-F03_1</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="64200000"/>
