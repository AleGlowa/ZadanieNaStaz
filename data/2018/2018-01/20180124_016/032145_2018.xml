                  <P>24 förlängning/ar á 1 månader</P>
               </RENEWAL_DESCR>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
