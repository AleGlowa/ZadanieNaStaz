                  <AC_CRITERION>Cena</AC_CRITERION>
                  <AC_WEIGHTING>60</AC_WEIGHTING>
               </AC_COST>
               <AC_COST>
                  <AC_CRITERION>Doświadczenie</AC_CRITERION>
                  <AC_WEIGHTING>20</AC_WEIGHTING>
               </AC_COST>
               <AC_COST>
                  <AC_CRITERION>Kryterium społeczne</AC_CRITERION>
                  <AC_WEIGHTING>20</AC_WEIGHTING>
               </AC_COST>
               <NO_OPTIONS/>
               <EU_PROGR_RELATED>POWR.01.03.01-IP.03-00-001/17</EU_PROGR_RELATED>
            </OBJECT_DESCR>
