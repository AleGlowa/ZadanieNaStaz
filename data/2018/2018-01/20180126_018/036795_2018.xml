                  <AC_WEIGHTING>5</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Qualität der vorgesehenen Projektorganisation: Datenmanagement</AC_CRITERION>
                  <AC_WEIGHTING>5</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>70</AC_WEIGHTING>
               </AC_PRICE>
