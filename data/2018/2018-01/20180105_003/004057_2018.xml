                     <AC_CRITERION>Laatupisteet</AC_CRITERION>
                     <AC_WEIGHTING>60</AC_WEIGHTING>
                  </AC_QUALITY>
                  <AC_PRICE>
                     <AC_WEIGHTING>40</AC_WEIGHTING>
                  </AC_PRICE>
               </AC>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
