            <SHORT_DESCR>
               <P>Predmet javnega naročila je prevzem, odvoz in predelava biorazgradljivih odpadkov.</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">194400.00</VAL_TOTAL>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="90510000"/>
