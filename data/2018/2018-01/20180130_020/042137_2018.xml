                  <AC_WEIGHTING>20 %</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Sustainability</AC_CRITERION>
                  <AC_WEIGHTING>5 %</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>50 %</AC_WEIGHTING>
               </AC_PRICE>
