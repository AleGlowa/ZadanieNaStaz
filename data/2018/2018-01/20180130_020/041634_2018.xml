         <OBJECT_CONTRACT>
            <TITLE>
               <P>Raamovereenkomst reagentia tbv HLA typeringen</P>
            </TITLE>
            <REFERENCE_NUMBER>18EA008</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="33696200"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Raamovereenkomst reagentia tbv HLA typeringen.</P>
            </SHORT_DESCR>
            <VAL_RANGE_TOTAL CURRENCY="EUR">
