         <OBJECT_CONTRACT>
            <TITLE>
               <P>Kuusalu teepiirkonna riigiteede korrashoiu korraldamine aastatel 2019–2021</P>
            </TITLE>
            <REFERENCE_NUMBER>194307</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="45233139"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="WORKS"/>
            <SHORT_DESCR>
               <P>Riigimaanteede aastaringne hooldamine Kuusalu teepiirkonnas vastavalt riigihanke alusdokumentides toodud tingimustele.</P>
            </SHORT_DESCR>
            <NO_LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="45233139"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
