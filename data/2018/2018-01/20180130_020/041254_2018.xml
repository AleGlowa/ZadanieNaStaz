         <OBJECT_CONTRACT>
            <TITLE>
               <P>Accord-cadre mono-attributaire à bons de commande pour l'évolution de l'infrastructure et des liaisons réseau informatique de la mairie de Colmar (123)</P>
            </TITLE>
            <CPV_MAIN>
               <CPV_CODE CODE="32424000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SUPPLIES"/>
            <SHORT_DESCR>
               <P>Evolution de l'infrastructure et des liaisons réseau informatique de la mairie de Colmar.</P>
            </SHORT_DESCR>
            <LOT_DIVISION>
               <LOT_ALL/>
            </LOT_DIVISION>
            <OBJECT_DESCR ITEM="1">
               <TITLE>
                  <P>Evolution de l'infrastructure réseau</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="32424000"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="FRF12"/>
               <SHORT_DESCR>
                  <P>Evolution de l'infrastructure réseau.</P>
                  <P>Accord-cadre à bons de commande sans montant minimum annuel ni montant maximum annuel.</P>
                  <P>À titre indicatif, le montant estimé annuel est de 120 000 EUR HT (soit 480 000 EUR HT sur 4 ans).</P>
               </SHORT_DESCR>
               <AC_PROCUREMENT_DOC/>
               <VAL_OBJECT CURRENCY="EUR">480000</VAL_OBJECT>
               <DURATION TYPE="MONTH">12</DURATION>
               <RENEWAL/>
               <RENEWAL_DESCR>
                  <P>L'accord-cadre sera reconductible annuellement de manière tacite 3 fois sans que la durée totale n'excède 4 ans.</P>
               </RENEWAL_DESCR>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
            <OBJECT_DESCR ITEM="2">
               <TITLE>
                  <P>Evolution des interconnexions des bâtiments</P>
               </TITLE>
               <LOT_NO>2</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="30236000"/>
               </CPV_ADDITIONAL>
               <n2016:NUTS CODE="FRF12"/>
               <SHORT_DESCR>
                  <P>Evolution des interconnexions des bâtiments.</P>
                  <P>Accord-cadre à bons de commande sans montant minimum annuel ni montant maximum annuel.</P>
                  <P>À titre indicatif le montant estimé annuel est de 80 000 EUR HT (soit 320 000 EUR HT sur 4 ans).</P>
               </SHORT_DESCR>
               <AC_PROCUREMENT_DOC/>
               <VAL_OBJECT CURRENCY="EUR">320000</VAL_OBJECT>
