         <OBJECT_CONTRACT>
            <TITLE>
               <P>Generalsanierung Egbert-Gymnasium Münsterschwarzach - LV 03 Vorabmaßnahmen HLS</P>
            </TITLE>
            <CPV_MAIN>
               <CPV_CODE CODE="45330000"/>
