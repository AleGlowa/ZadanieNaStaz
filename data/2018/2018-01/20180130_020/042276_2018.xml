            <REFERENCE_NUMBER>2017/122</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="90910000"/>
            </CPV_MAIN>
            <TYPE_CONTRACT CTYPE="SERVICES"/>
