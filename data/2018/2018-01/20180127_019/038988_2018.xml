            <SHORT_DESCR>
               <P>Lieferung von verschiedenen Einsatzfahrzeugen für den Brand- und Katastrophenschutz</P>
            </SHORT_DESCR>
            <VAL_TOTAL CURRENCY="EUR">0.01</VAL_TOTAL>
            <LOT_DIVISION/>
            <OBJECT_DESCR ITEM="1">
               <TITLE>
                  <P>Hilfeleistungslöschgruppenfahrzeug HLF 20</P>
               </TITLE>
               <LOT_NO>1</LOT_NO>
               <CPV_ADDITIONAL>
                  <CPV_CODE CODE="34144210"/>
               </CPV_ADDITIONAL>
               <CPV_ADDITIONAL>
