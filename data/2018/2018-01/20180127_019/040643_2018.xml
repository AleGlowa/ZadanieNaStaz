               <RENEWAL/>
               <RENEWAL_DESCR>
                  <P>Option on 1 year + 1 year.</P>
               </RENEWAL_DESCR>
               <NO_ACCEPTED_VARIANTS/>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
         </OBJECT_CONTRACT>
