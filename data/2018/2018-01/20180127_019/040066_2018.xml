                     <AC_WEIGHTING>65</AC_WEIGHTING>
                  </AC_COST>
               </AC>
               <NO_OPTIONS/>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>
         </OBJECT_CONTRACT>
