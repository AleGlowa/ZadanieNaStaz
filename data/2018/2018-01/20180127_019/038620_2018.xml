         <OBJECT_CONTRACT>
            <TITLE>
               <P>Küchentechnik (4/031/2017 - H35 LV 400)</P>
            </TITLE>
            <REFERENCE_NUMBER>2017031</REFERENCE_NUMBER>
            <CPV_MAIN>
