               <SHORT_DESCR>
                  <P>Der agtes udbudt en eneleverandøraftale som en rammeaftale om levering af brændstoffer til Udbyders tanke.</P>
               </SHORT_DESCR>
               <AC_PROCUREMENT_DOC/>
               <VAL_OBJECT CURRENCY="DKK">48728000.00</VAL_OBJECT>
               <DURATION TYPE="MONTH">48</DURATION>
               <NO_EU_PROGR_RELATED/>
