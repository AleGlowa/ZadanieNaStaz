                  <AC_WEIGHTING>20</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_COST>
                  <AC_CRITERION>Honorar</AC_CRITERION>
                  <AC_WEIGHTING>10</AC_WEIGHTING>
               </AC_COST>
               <DATE_START>2018-07-01</DATE_START>
               <DATE_END>2024-12-31</DATE_END>
