         <OBJECT_CONTRACT>
            <TITLE>
               <P>Remont turbozespołu oraz urządzeń pomocniczych bloku nr 5 w Elektrowni Dolna Odra.</P>
            </TITLE>
            <REFERENCE_NUMBER>GEK/PMR-ELD/09368/2016</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="45315200"/>
