               </AC_QUALITY>
               <AC_QUALITY>
                  <AC_CRITERION>Cohérence des propositions d'organisation des prestations</AC_CRITERION>
                  <AC_WEIGHTING>40</AC_WEIGHTING>
               </AC_QUALITY>
               <AC_PRICE>
                  <AC_WEIGHTING>45</AC_WEIGHTING>
               </AC_PRICE>
               <VAL_OBJECT CURRENCY="EUR">487744</VAL_OBJECT>
               <DURATION TYPE="MONTH">7</DURATION>
